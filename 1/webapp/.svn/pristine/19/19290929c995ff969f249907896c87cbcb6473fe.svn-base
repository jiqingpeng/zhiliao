@charset "utf-8";

/* reset */
body,h1,h2,h3,h4,h5,p,dd,ul,form,input,textarea,th,td,select,button { margin:0; padding:0; }
em { font-style:normal; }
li { list-style:none; }
a { text-decoration:none; }
img { border:none; vertical-align:top; }
table { border-collapse:collapse; }
input,textarea { outline:none; }
textarea { resize:none; overflow:auto; }
body { font-size:14px; font-family:"SimSun","宋体","Arial Narrow",HELVETICA; background-color: #e8eaf0; }
/* end reset */

body {
	min-width: 980px;
	min-height: 670px;
}
.box{
	top: 156px;
}
.user-nav{
    font-size:18px;
	color:#333;
	height: 45px;
	line-height: 45px;
	border-bottom:1px #c9924b solid; 
	margin-bottom: 20px;
	padding-left: 14px;
}

.red {
	color: red;
}
.prejoincon {
	background: #fff;
	width: 1100px;
	margin: 20px auto;
	padding-top: 1px;
}

.joincon {
	background: #fff;
	/*width: 1100px;*/
	min-height: 800px;
	padding-top: 1px;
}
.activname {
	height: 56px;
	font-size: 22px;
	line-height: 56px;
	text-align: center;
	margin-top: 14px;
	font-weight: bold;
	color: #333;
}
.jcon {
	width: 960px;
	min-height: 100px;
	margin: 14px 70px 0 70px;
}
.jconheader {
	height: 48px;
	border-bottom: 1px solid #ccc;
}


.jcontil {
	display: inline-block;
	width: 150px;
	height: 46px;
	line-height: 46px;
	text-align: center;
	border-bottom: 2px solid #2bc6fb;
	position: relative;
	top: 2px;
}
.jconnum {
	display: inline-block;
	height: 23px;
	width: 23px;
	line-height: 23px;
	background: #ccc;
	color: #fff;
	margin-right: 10px;
	border-radius: 11px 11px;
}


.jcondetail li {
	height: 58px;
	line-height: 58px;
	position: relative;
}
.til {
	display: inline-block;
	width: 108px;
	height: 58px;
	line-height: 58px;
	text-align: right;
}
/*.red {
	color: red;
	margin-right: 2px;
}*/
.inp {
	height: 36px;
	padding-left: 10px;
	width: 342px;
	border: 1px solid #ddd;
	border-radius: 3px 3px;
	margin-left: 12px;
}

.radioinp {
	height: 22px;
	width: 22px;
	margin: 0 6px 0 18px !important;
	position: relative;
	top: 5px;
}

.notUse { /* 不需要的样式 */
	background: #ddd;
}
.notUse label { 
	color: #999;
}


.addfield {
	height: 60px;
}
.addfield button {
	font-size: 28px;
	height: 30px;
	width: 30px;
	border-radius: 15px;
	border: 1px solid #ddd;
	line-height: 22px;
	font-weight: bold;
	margin-left: 430px;
	margin-top: 20px;
}


/* 右侧操作部分样式 */
.filedopt {
	margin: 0 12px;
	position: absolute;
	right: 300px;
}
.checkinp {
	margin: 0 2px !important;
	
}
.moveopt {
	position: absolute;
	right: 220px;
}
.moveopt img {
	height: 22px;
	width: 22px;
	cursor: pointer;
	position: relative;
	top: 16px;
	margin-right: 12px;
}

/* 底部按钮 */
.joinbtn {
	height: 68px;
	line-height: 68px;

	border-top: 1px solid #ccc;
	text-align: center;
}

.joinbtn button {
	margin-right: 24px;
}
.preview {
	margin-left: 180px;
}