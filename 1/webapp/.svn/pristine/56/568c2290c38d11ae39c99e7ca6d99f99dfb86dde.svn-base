			$$.module.address.source.tHote_hotData.U =data.U;
			$$.module.address.source.tHote_hotData.V =data.V;
			$$.module.address.source.tHote_hotData.W =data.W;
			$$.module.address.source.tHote_hotData.X =data.X;
			$$.module.address.source.tHote_hotData.Z =data.Z;
			$$.module.address.source.tHote_hotData.Y =data.Y;
		})();
		//火车查询
		$('#trainBtn').on('click',function(){
			var formC=$('#traincityid').val();
			var toC=$('#traingetcityid').val();
			var formT=$('#trainDate').val();
			if(seachValidation(formC,toC,formT)){
				//火车票查询
				var oa=$('#traincityid').val();
				var aa=$('#traingetcityid').val();
				var fDate=$('#trainDate').val();
				window.location.href="../index/train.html?oa="+oa+"&aa="+aa+"&fDate="+fDate+"&actId="+$("#id").val();
			};
		});
		//判断交通状态
		if(result.data.trainOrder.length==0){
			$('.traffic').css('display','block');
		};
		console.log(result);
		//	初始化result
		// var initResult=result;
		// 生成订单列表
		var list=result.data.trainOrder;
		for(var i=0;i<list.length;i++){
			$('.listboxModel').before($('.listboxModel').html());
			//订单id
			$('.listboxModel').prev().find('.orderId').val(list[i].id);
			//车次
			$('.listboxModel').prev().find('.trainNo').html(list[i].trainNo);
			//出发车站
			$('.listboxModel').prev().find('.fromStation').html(list[i].fromStation);
			//到达车站
			$('.listboxModel').prev().find('.toStation').html(list[i].toStation);
			//出发时间
			$('.listboxModel').prev().find('.departureTime').html(list[i].departureTime);
			//到达时间
			$('.listboxModel').prev().find('.arrivalTime').html(list[i].arrivalTime);
			//订单号
			$('.listboxModel').prev().find('.outOrderNo').html('订单号：'+list[i].outOrderNo);
			//订单金额
			// $('.listboxModel').prev().find('.price').html('￥'+list[i].orderPrice);
			//状态
			$('.listboxModel').prev().find('.tickMessage').html(list[i].orderStateValue);
			//支付剩余时间
			if(list[i].orderState==1||list[i].orderState==3){
				$('.listboxModel').prev().find('.lastPay').css({
					"display":"block"
				});
				//倒计时
				function Timer(time){
					var timer=null;
		    		clearInterval(timer);
		    		var timestamp = Date.parse(new Date());
				    timestamp = timestamp / 1000;
				    var i=(time/1000)+2700-timestamp;
		    		function show(){
						var s=i;
						var h=parseInt(s/3600);
						s%=3600;
						var m=parseInt(s/60);
						s%=60;
						s=parseInt(s);
						var t =Dou(h)+':'+Dou(m)+':'+Dou(s);
						$('.listboxModel').prev().find('.payTime').html(t);
						if(i<=0){
							clearInterval(timer);
						};
						i--;
					};
					show()
					timer=setInterval(show,1000);
				};
				Timer(list[i].ctime);
				function Dou(n){
					return n<10 ?'0'+n :''+n; 
				};
			};
			//联系人
			$('.listboxModel').prev().find('.personName').html('联系人:'+list[i].person);
			//联系电话
			// $('.listboxModel').prev().find('.cellPhone').html('联系电话:'+list[i].cellphone);
			
			//取消订单
			if(list[i].orderState==5||list[i].orderState==7||list[i].orderState==8){
				$('.listboxModel').prev().find('.cancelOrder').css({
					'background':'#ccc',
					'border-color':'#ccc'
				});
			};
			//支付订单
			if(list[i].orderState==5||list[i].orderState==7||list[i].orderState==8){
				$('.listboxModel').prev().find('.payOrder').css({
					'background':'#ccc',
					'border-color':'#ccc'
				});
			};
			var detailList=list[i].lyTrainPassengerList;
			for(var j=0;j<detailList.length;j++){
				$('.listboxModel').prev().find('.passengerTit').after('<ul class="clearfix passenger"><input type="hidden" class="passId" value="'+detailList[j].id+'"><li class="fl passName">'+detailList[j].passengerName+'</li><li class="fl passSeat">'+detailList[j].seatClassName+detailList[j].seatNo+'</li><li class="fl passTicket">票价:<span class="num">￥'+detailList[j].ticketPrice+'</span></li><li class="fr"><a class="cancelTicket orderBtn" href="javascript:;">退票</a></li></ul> ');
			};
		};
		// 生成订单列表**************end
	};
};
