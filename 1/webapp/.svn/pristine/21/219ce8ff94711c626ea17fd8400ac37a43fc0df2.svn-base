<script>
	$(function(){
		//init获取页面信息
		var url=basePath+"index/init.do";
		$.ajax({
			url: url,
			type: 'POST',
			dataType: 'json',
			xhrFields: {
       			withCredentials: true
    		},
    		crossDomain: true,
			success:function(result){
				console.log(result)
				//循环行业类型
				for(var i=0;i<9;i++){
					//会议导航
					$('.conferenceNav').append('<li class="fl"><input type="hidden" dataType="1" value="'+result.data.industy[i].value+'"/><span class="on_cat">'+result.data.industy[i].name+'</span></li>');
					//会展导航
					$('.exhibitionNav').append('<li class="fl"><input type="hidden" dataType="2" value="'+result.data.industy[i].value+'"><span class="on_cat">'+result.data.industy[i].name+'</span></li>');
					//培训导航
					$('.trainNav').append('<li class="fl"><input type="hidden" dataType="3" value="'+result.data.industy[i].value+'"><span class="on_cat">'+result.data.industy[i].name+'</span></li>');
					//大赛导航
					$('.tounamentNav').append('<li class="fl"><input type="hidden" dataType="4" value="'+result.data.industy[i].value+'"><span class="on_cat">'+result.data.industy[i].name+'</span></li>');
				};
				//会议
				for(var i=0;i<result.data.conference.length;i++){
					if(i>=10){
						break;
					}
					//截取时间
					var stime=result.data.conference[i].stime.substr(0,11);
					//填充数据
					var str='<li class="clearfix"><span class="fl dot"></span><input class="fl" type="hidden" value="'+result.data.conference[i].id+'"/><span class="time fl">'+stime+'</span><span class="city fl"><i>'+result.data.conference[i].city+'</i></span><a target="_blank" class="on_mess fl" >'+result.data.conference[i].name+'</a></p></li>';
					$('.conference').append(str);
				};
				//会展
				for(var i=0;i<result.data.exhibition.length;i++){
					if(i>=10){
						break;
					}
					//截取时间
					var stime=result.data.exhibition[i].stime.substr(0,11);
					//填充数据
					var str='<li class="clearfix"><span class="fl dot"></span><input class="fl" type="hidden" value="'+result.data.exhibition[i].id+'"/><span class="time fl">'+stime+'</span><span class="city fl"><i>'+result.data.exhibition[i].city+'</i></span><a target="_blank" class="on_mess fl" >'+result.data.exhibition[i].name+'</a></p></li>';
					$('.exhibition').append(str);
				};
				//培训
				for(var i=0;i<result.data.train.length;i++){
					if(i>=10){
						break;
					}
					//截取时间
					stime=result.data.train[i].stime.substr(0,11);
					//填充数据
					var str='<li class="clearfix"><span class="fl dot"></span><input class="fl" type="hidden" value="'+result.data.train[i].id+'"/><span class="time fl">'+stime+'</span><span class="city fl"><i>'+result.data.train[i].city+'</i></span><a target="_blank" class="on_mess fl" >'+result.data.train[i].name+'</a></p></li>';
					$('.train').append(str);
				};
				//大赛
				for(var i=0;i<result.data.tounament.length;i++){
					if(i>=10){
						break;
					}
					//截取时间
					var stime=result.data.tounament[i].stime.substr(0,11);
					//填充数据
					var str='<li class="clearfix"><span class="fl dot"></span><input class="fl" type="hidden" value="'+result.data.tounament[i].id+'"/><span class="time fl">'+result.data.tounament[i].stime.substr(0,11)+'</span><span class="city fl"><i>'+result.data.tounament[i].city+'</i></span><a target="_blank" class="on_mess fl" >'+result.data.tounament[i].name+'</a></p></li>';
					// var str='1'
					$('.tounament').append(str);
				};
				//登陆状态
				if(result.data.user){
					$('.log_group').addClass('active');
					$('.log_group2').removeClass('active');
					$('#user_name').html(result.data.user.name);
				};
			},
			error:function(){
				alert('请求失败');
			}
		});
		//链接跳活动详情
		$(document).on('click','.on_mess',function(){
			$(this).attr("href",'detail.html?location='+$(this).prev().prev().prev().val());
			// window.location.href='detail.html?location='+$(this).prev().prev().prev().val();
		});
		//跳活动分类页
		$(document).on('click','.on_cat',function(){
			// alert(1)
			// $(this).attr('href','cation.html?val='+$(this).prev().val()+'&dataType='+$(this).prev().dataType())
			window.location.href='cation.html?type='+$(this).prev().attr('dataType')+'&trade='+$(this).prev().val();
		});
		//查看更多
		$(document).on('click','.more',function(){
			window.location.href='cation.html?type='+$(this).attr('dataType')+'&trade='+$(this).attr('dataTrade');
		});
		//登陆样式
		// $('.login').on('mouseover',function(){
		// 	$('.login_box').css('display','block')
		// });
		// $('.login').on('click',function(){
		// 	$('.login_box').css('display','block')
		// });
		$('#login').click(function(){
			login();
		});
		function login(){
				url=basePath+"user/login/check.do";
				var mobile = $('.user').val();
				var pwd=$('.password').val();
				var pwdMd5=$.md5($('.password').val());
				// 验证手机号
				if (mobile == null || mobile == '') {
					myAlert('账号不能为空!','slow');
					return false;
				}else{
					if(!(/^1[34578]\d{9}$/.test(mobile))){ 
			        	myAlert('账号格式不正确！','slow');  
			        	return false; 
			        };
				};
				// 验证密码
				if (pwd == null || pwd == '') {
					myAlert('密码不能为空!','slow');
					return false;
				};
				$.ajax({
					url: url,
					data: {
						mobile: mobile,
						pwd:pwdMd5 
						// phone: '13633334444',
						// pwd:'asd' 
					},
					type: 'POST',
					dataType: 'json',
					xhrFields: {
	           			withCredentials: true
	        		},
	        		crossDomain: true,
					success:function(result){
						console.log(result)
						if(result.flag){
							window.location.href='../user/activityList.html';
						}else{
							// $('.login_message').css('display','block')
							// $('.login_message').html(result.msg)
							myAlert(result.msg,'slow');
						};
					}
				});
		};
		//验证码显示
		// $('.password').focus(function(){
		// 	var user=$('.user').val();
		// 	if(user!=''){
		// 		$('.regCode').removeClass('active');
		// 	}
		// });
		//键盘登陆
		$(document).keypress(function(e) {  
			// 回车键事件  
	        if(e.which == 13) {
	        	login(); 
	        };	  
		});
		//注销
		loginOut('#login_out');
	});
</script>
