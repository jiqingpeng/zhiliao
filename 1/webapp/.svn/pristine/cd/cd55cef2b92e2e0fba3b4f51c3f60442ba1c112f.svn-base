#wrapper{
	width:94%;
	padding:0 3%;
	position:absolute;
	top:50px;
	bottom:40px;
	overflow:auto;
	z-index:1;
}
#scroller {
	position: absolute;
}
#pullDown,#pullUp{
	text-align:center;
	// display: none;
}
