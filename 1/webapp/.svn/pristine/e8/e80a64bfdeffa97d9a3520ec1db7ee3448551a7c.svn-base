<script>
	$(function(){
		//init获取页面信息和登陆状态
		var url=basePath+"index/init.do";
		$.ajax({
			url: url,
			type: 'GET',
			dataType: 'json',
			xhrFields: {
       			withCredentials: true
    		},
    		crossDomain: true,
			success:function(result){
				console.log(result)
				//行业类型
				for(var i=0;i<result.data.industy.length;i++){
					$('.hlist_message').append('<li class="fl"><a href="#">'+result.data.industy[i].name+'</a></li>');
				};
				//行业分类信息
				var pageData=[]; 
				for(var i=0;i<result.data.activity.length;i++){
					var stime= result.data.activity[i].stime.substr(0,11); 
				    var str='<li class="clearfix"><input type="hidden" value="'+result.data.activity[i].id+'"/><span class="time fl">'+stime+'</span><a href="#" class="fl mess on_mess">'+result.data.activity[i].name+'</a><span class="place" class="fl">'+result.data.activity[i].city+'</span></li>';
				    pageData.push(str);  
				};
				//分页
				function pageShow(){  
				    var Count = pageData.length;//记录条数  
				    var PageSize=10;//设置每页示数目  
				    var PageCount=Math.ceil(Count/PageSize);//计算总页数  
				    var currentPage =1;//当前页，默认为1。
				    var selectPage=currentPage;
				    console.log(selectPage)  
				    //默认显示
				    for(i=(currentPage-1)*PageSize;i<PageSize*currentPage;i++){    
				        $('.actdetail').append(pageData[i]);  
				    }
				    $('.pageList li a').eq(currentPage-1).addClass('org'); 
				    //显示选择页的内容  
				    $('.pageList li a').on('click',function(){  
				        selectPage=($(this).parent().index()+1);
				        page();  
				    });
				    //每页
				    function page(){
				    	console.log(selectPage)
				        $('.pageList li a').removeClass('org');  
				        $('.pageList li a').eq(selectPage-1).addClass('org'); 
				        $('.actdetail').html('');
				        for(i=(selectPage-1)*PageSize;i<PageSize*selectPage;i++){  
				            $('.actdetail').append(pageData[i]);  
				        };
				    }
				    //上一页
				    $('.prev').click(function(){
				    	selectPage--;
				    	if(selectPage<1){
				    		alert('已到首页');
				    		firstpage();
				    	}
				    	page();
				    });
				    //下一页 
				    $('.next').click(function(){
				    	selectPage++;
				    	if(selectPage>9){
				    		alert('已到尾页');
				    		lastpage();
				    	};
				    	page();
				    });
				    //首页
				    $('.first_page').click(firstpage)
				    function firstpage(){
					        selectPage=1;
					        console.log(selectPage);
					        page();
					}
				    //尾页
				    $('.last_page').click(lastpage)
				    function lastpage(){
				        selectPage=$('.pageList li').length;
				        console.log(selectPage);
				        page();
				    }
				    //键盘选择页面
					$(document).keypress(function(e) {  
						// 回车键事件  
				        if(e.which == 13) {
				        	if($('.kup').val()>10){
				        		alert('最大页数为10')

				        	}else if($('.kup').val()<1){
				        		alert('最小页数为1')
				        	}
				        	else{
				        		$('.actdetail').html('');
					        	selectPage=currentPage=$('.kup').val();
								console.log(selectPage);
								page();
							} 
				        }	  
					});
				};
				pageShow();
				//用户登陆状态信息
				if(result.data.user){
					$('#user_name').html(result.data.user.name);
					$('#login_out').html('注销');
				}
			}
		});
		//跳活动详情
		$(document).on('click','.on_mess',function(){
			window.location.href='detail.html?location='+$(this).prev().prev().val();
		});
		//注销
		$('#login_out').click(function(){
			url=basePath+"login/out.do"
			url: url,
			$.ajax({
				url: url,
				type: 'GET',
				dataType: 'json',
				xhrFields: {
           			withCredentials: true
        		},
        		crossDomain: true,
				success:function(data){
					console.log(data.url)
					window.location.href='index.html'
				}
			})
		});
	})
</script>
