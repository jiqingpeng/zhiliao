@charset "utf-8";

/* reset */
body,h1,h2,h3,h4,h5,p,dd,ul,form,input,textarea,th,td,select,button { margin:0; padding:0; }
em { font-style:normal; }
li { list-style:none; }
a { text-decoration:none; }
img { border:none; vertical-align:top; }
table { border-collapse:collapse; }
input,textarea { outline:none; }
textarea { resize:none; overflow:auto; }
body { font-size:14px; font-family:"SimSun","宋体","Arial Narrow",HELVETICA; background-color: #e8eaf0; }
/* end reset */

