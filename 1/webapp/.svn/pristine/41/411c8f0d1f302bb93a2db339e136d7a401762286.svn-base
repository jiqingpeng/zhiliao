<script type="text/javascript">
$(function(){
	$(".applyFor").on("click", function(){
		var bootValidator = $("#detailform").data("bootstrapValidator");
		bootValidator.validate();
		if(bootValidator.isValid()){	
			$.myAjax({
				url: basePath + "super/regist/check.do",
				data: $("#detailform").serialize(),
				success: function(result){
					if(result.flag){
						myConfirm("申请成功,请保持电话24小时畅通", function(){
							window.location.href = "../../index/index.html";
						});
						$(".applyFor").attr("disabled", true);
					}else{
						myConfirm(result.msg);
					}
				},
				error: function(){
					myAlert("请求失败!", "slow")
				}
			})
		}
	})
})
//表单验证
$("#detailform").bootstrapValidator({
        message: 'This value is not valid',
        feedbackIcons: {
            valid: 'glyphicon glyphicon-ok',
            invalid: 'glyphicon glyphicon-remove',
            validating: 'glyphicon glyphicon-refresh'
        },
        fields: {
            contactUser: {
                validators: {
                    notEmpty: {
                        message: '申请人不能为空'
                    }
                }
            },
            contactMobile: {
                validators: {
                    notEmpty: {
                        message: '手机不能为空'
                    },
                    regexp: {
                        regexp: /^1[34578]\d{9}$/,
                        message: '请输入正确的手机号！'
                    }
                }
            },
            contactDuty: {
                validators: {
                    notEmpty: {
                        message: '职务不能为空'
                    }
                }
            },
            name: {
                validators: {
                    notEmpty: {
                        message: '单位不能为空'
                    }
                }
            },
            address: {
                validators: {
                    notEmpty: {
                        message: '单位地址不能为空'
                    }
                }
            },
            contactEmail: {
                validators: {
                    notEmpty: {
                        message: '邮箱不能为空'
                    },
                    emailAddress: {
                        message: '请输入正确的邮箱地址！'
                    }
                }
            }
        }
});
