    <div id="contentBody" class="mops-table">
        <form id="proceForm">
            <input type="hidden" id="id" name="id"/>
            <table id="texttable">
            </table>
        </form>

        <div class="btncon">
            <button class="btn btn-warning save">保&nbsp存</button>
            <button class="btn btn-default cancel">取&nbsp消</button>
