<style>
    ul.timeline {
       list-style-type: none;
       background: url("../../img/user/timeLine4.png") repeat-y 120px;
       margin-left:20px;
    }
    ul.timeline li {
       position: relative;
       margin-bottom: 20px;
       color:#122e67;
    }
    ul.timeline li .time {
       position: absolute;
        width: 90px;
       text-align: right;
        left: 0;
        top: 10px;
        /*color: #999;*/
    }
    ul.timeline li .version {
        position: absolute;
        width: 290px;
        text-align: right;
        left: -200px;
        top: 30px;
        font-size: 30px;
       line-height: 50px;
       /*color: #fe6600;*/
        overflow: hidden;
    }
    ul.timeline li .number {
        position: absolute;
        background:#fe6600;
        width: 26px;
        height: 26px;
        border-radius: 50%;
        left: 112px;
        line-height: 26px;
        text-align: center;
        color: #fff;
        font-size: 18px;
    }
    ul.timeline li.alt .number {
        background:#cd7228;
    }
    ul.timeline li .content {
        margin-left: 180px;
        
    }
    ul.timeline li .content pre {
        /*background-color:#fe6600;*/
        padding: 20px;
        color: #122e67;
        font-size: 13px;
        line-height: 20px;
    }
    ul.timeline li.alt .content pre {
        background-color: #cd7228;
    }
</style>
</head>
