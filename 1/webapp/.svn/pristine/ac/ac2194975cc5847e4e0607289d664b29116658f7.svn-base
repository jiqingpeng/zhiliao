<!DOCTYPE html>
<html>
<head>
	<title>组织机构</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0,user-scalable=no">
	<link rel="stylesheet" type="text/css" href="../../css/common/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="../../css/weixin/style.css">
	<style type="text/css">
		.institutiona{
			width: 100%;
			/*background-color: #ccc;*/
		}
		.instiContent{
			padding-left: 30px;
		}
		.instiContent{
			border-bottom: 1px solid #f3940d;
		}
		.instiName{
			margin-top: 20px;
			font-size: 16px;
			font-weight: 700;
			margin-bottom: 5px;
		}
	</style>
</head>
<body>
	<iframe name="head" class="" marginwidth="0" marginheight="0" width="100%" height="58" src="header.html" frameborder="0"></iframe>
	<div class="institutiona">
		<div class="container">
			<div class="instiBox">
				<div class="instiName">
				</div>
				<div class="instiContent">
					<div class="content"></div>
				</div>
			</div>
		</div>	
	</div>
</body>
<script src="../../js/jquery.min.js"></script>
<script src="../../js/bootstrap.min.js"></script>
<script src="../../js/common/common.js"></script>
<script type="text/javascript">
	$.myAjax({
		url: basePath + "wx/islogin.do",
		dataType: "text",
		success: function(data){
			console.log(data);
			if(data == "notlogin"){
				var url = "index/activity.do";
				loginState(url, 13);
			}else{
				var url = "user/activity/get.do";
				loginState(url, 1);
			}
		}
	});
	
	function loginState(url, type){
		$.myAjax({
			url: basePath + url,
			data: {id: "25812d45fc27441ebacb6f9e3fe70322", type: type},
			success: function(result){
				console.log(result);
				if(result.flag){
					// console.log(result.data.activityOrgList[1].value[1]);
					var activityOrgList = result.data.activityOrgList;
					var str="";
					for (var i = 0; i < activityOrgList.length; i++) {
							var string="";
							for(var j = 0; j < activityOrgList[i].value.length; j++){
								console.log(activityOrgList[i].value[j]);
								string += '<div class="content">' +
												activityOrgList[i].value[j] +
											'</div>';
								
							}
							$(".instiContent").html(string);
							str += '<div class="instiName">'+
										activityOrgList[i].name +
									'</div>'+
									'<div class="instiContent">' +
										string +
									'</div>';		
						};
						$(".instiBox").html(str);	
				}else{
					
				}
			}
		});
	}

</script>
</html>