<script type="text/javascript">
$(function(){
	// 获取左侧菜单列表数据(参数为请求地址)
	initMenu('../../../js/data/administrator.json','活动管理');

	var str = '<span class="filedopt">'+
					'<input type="checkbox" class="must checkinp"/>必填'+
					'<input type="checkbox" class="optional checkinp"/>选填'+
					'<input type="checkbox" class="noneed checkinp"/>不需要'+
				'</span>'+
				'<span class="moveopt">'+
					'<img src="../../../img/moveup.png" title="上移" class="moveup"/>'+
					'<img src="../../../img/movedown.png" title="下移" class="movedown"/>'+
				'</span>';
	$(".jconeditdetail li").append(str);

	var noNeed = $(".noneed");
	noNeed.each(function(index){
		var flag = true;
		$(this).click(function(){
			var parentE = $(this).parent().parent();
			if(flag){
				$(parentE).attr('class','notUse');
				$(parentE).find('.type').css('color','#999');
				$(parentE).find('img').attr('disabled','disabled').css('cursor','not-allowed');
				flag = !flag;
			} else {
				$(parentE).removeClass('notUse');
				$(parentE).find('img').attr('disabled',false).css('cursor','pointer');
				$(parentE).find('.type').css('color','#333');
				flag = !flag;
			}
		});
	});

	(function(){
		var aUp = $(".moveup");
		aUp.each(function(index){
			$(this).click(function(){
				var parentE = $(this).parent().parent();
				if($(parentE).prev().is('li')){
					var prevE = $(parentE).prev();
					$(parentE).insertBefore($(prevE));
				} else {
					alert("已是第一个！");
				}
			});
		});
	})();
	(function(){
		var aUp = $(".movedown");
		aUp.each(function(index){
			$(this).click(function(){
				var parentE = $(this).parent().parent();
				if($(parentE).next().is('li')){
					var nextE = $(parentE).next();
					$(nextE).insertBefore($(parentE));
				} else {
					alert("已是最后一个！");
				}
			});
		});
	})();

	$(".morefiled").click(function(){
		if ($(this).html() == '收起') {
			$(this).html('查看更多字段');
			$(this).prev().css({'max-height':'500px','min-height':'100px','overflow':'hidden'});
		} else {
			$(this).html('收起');
			$(this).prev().css({'max-height':'9999px','min-height':'500px'});
		}
	});
	$(".addfieldbtn").click(function(){
		alert('自定义添加字段！！');
	});

	var urltemp ='admin/activityregconfig/list   save    del  ';
	var url = basePath + 'admin/activityregconfig/list.do';
	$.ajax({
			url: url,
			type: "POST",
			data: {id:'9c2d2f6a84e84ee882b73d66a7b8a7f6'},
			dataType: "json",
			xhrFields: {
				withCredentials: true
			},
			crossDomain: true,
			success: function(result){
				console.log(result);
				
			},
			error: function(){
				console.log("请求失败");
			}
		})
});
</script>
