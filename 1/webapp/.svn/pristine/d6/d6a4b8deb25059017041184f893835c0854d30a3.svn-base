		body,ul,li {
		     padding:0;
		     margin:0;
		     border:0;
    	}
   
	    body {
	     font-size:12px;
	     -webkit-user-select:none;
	     -webkit-text-size-adjust:none;
	     font-family:helvetica;
	     background:#ccc;
	    }
   		#header .mess-foot{
			padding:2px;
		}
	    #footer {
		     position:absolute;
		     bottom:0; left:0;
		     width:100%;
		     height:48px;
		     background-image:-webkit-gradient(linear, 0 0, 0 100%, color-stop(0, #999), color-stop(0.02, #666), color-stop(1, #222));
		     background-image:-moz-linear-gradient(top, #999, #666 2%, #222);
		     background-image:-o-linear-gradient(top, #999, #666 2%, #222);
		     padding:0;
		     border-top:1px solid #444;
	    }
   		
	    #wrapper {
	     position:absolute; z-index:1;
	     top:100px; 
	     bottom:1px; 
	     left:0;
	     width:100%;
	    /* background:#555;*/
	     overflow:auto;
	    }
   
	    #scroller {
	     position:relative;
	    /* -webkit-touch-callout:none;*/
	     -webkit-tap-highlight-color:rgba(0,0,0,0);
	  	float:left;
	     width:100%;
	     padding:0;
	    }
	    
		#pullDown, #pullUp {
		    background:#fff;
		    height:40px;
		    line-height:40px;
		    padding:5px 10px;
		    border-bottom:1px solid #ccc;
		    font-weight:bold;
		    font-size:14px;
		    color:#ccc;
		    text-align: center;
		}
		.floor{
			width:100%;
			padding:10px;
			margin-bottom:1px;
			background:#fff;
		}
		.floor .talk-txt{
			width:86%;
		}
		.floor .mess-foot{
			margin-left:14%;
		}
		.mess-head .mess-tit{
			width:86%;
		}
		.mess-head .mess-icon{
			width:30px;
			padding:0;
		}
		img{
			width:30px;
			height:30px;
			border-radius: 50%;
			background:#f0f0f0;
		}
		.mess-head .mess-tit .mess-time{
			
		}
		.tit-cont{
			padding-top:10px;
			text-align:center;
		}
		
		.mess-foot .item{
			padding:0;
			float:right;
		}
		.mess-foot .item img{
			/*border:1px solid red;*/
			display: inline-block;
			width:18px;
			height:20px;
			vertical-align: top;
		}
		
		.mess-talk{
			width:100%;
			height:30px;
			position:fixed;
			left:0;
			bottom:1px;
			display:none;
			z-index:5;
			padding:0 2%;
			background:#fff;
		}
		.mess-talk .mess-txt{
			width:100%;
		}
		.mess-talk .mess-txt img{
			width:30px;
			height:30px;
			padding:0;
		}
		.mess-talk .mess-txt input{
			height:30px;
			padding:0;
		}
		.mess-foot .mess-time{
			padding:0;
		}
		.mess-talk-group{
			background:#ccc;
		}
		.mess-talk-group .page-more{
			margin-left:2%;
			margin-top:5px;
		}
		.mess-talk-group img{
			width:30px;
			height:30px;
			border-radius: 50%;
		}
		.mess-talk-group .talk-txt{
			width:86%;
			/*overflow:hidden;*/
		}
		.talk-btn{
			display: inline-block;
			width:64px;
			height:30px;
			line-height: 30px;
			background:#f60;
			color:#fff;
			text-align:center;
			border-radius:2px;
			float:right;
		}
		.mess-child .second{
			display:none;
		}
		.mess-child .child-floorw{
			width:98%;
			padding:2%;
			margin-top:10px;
		}
		.mess-child .child-floorw .talk-txt{
			width:86%;
			word-wrap:break-word ;
		}
		.reply{
			display: inline-block;
			width:100%;
			text-align: right;
		}
		.f60{
			color:#f60;
		}