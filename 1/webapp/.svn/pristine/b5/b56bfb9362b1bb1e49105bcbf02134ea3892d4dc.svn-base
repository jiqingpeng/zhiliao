</body>
<script src="../../js/jquery.min.js"></script>
<script src="../../js/bootstrap.min.js"></script>
<script src="../../js/bootstrap-table.min.js"></script>
<script src="../../js/bootstrap-table-zh-CN.min.js"></script>
<script src="../../js/tableExport.js"></script>
<script src="../../js/bootstrap-table-export.js"></script>
<script src="../../js/mng/mng.js"></script>
<script type="text/javascript">
    $(function () {
        loding();
        init();
        tableInit();
    });

    function init() {
        $
            .myAjax({
                url: basePath + "monitor/activity/list.do",
                type: "POST",
                async: false,
                dataType: "JSON",
                success: function (result) {
                    lodingDown();
                    if (result.flag) {
                        $('#activity-table').bootstrapTable('load', result.data.activityList);
                    } else {
                        alert(result.msg);
                    }
                },
                error: function (result) {
                    alert(result.msg);
                }
            });
    }

    function tableInit() {
        $('#activity-table').bootstrapTable({
            dataType: 'json', //数据格式
            dataLocale: 'zh-US', //表格汉化
            toolbar: '#toolbar',
            striped: true, //是否显示行间隔色
            cache: false, //是否使用缓存，默认为true，所以一般情况下需要设置一下这个属性（*）
            pagination: true, //是否显示分页（*）
            sortable: true, //是否启用排序
            sortOrder: "desc", //排序方式
            search: true,
            showExport: true,
            searchFormatter: false,
            showRefresh: false, //是否显示刷新按钮
            cellStyle: true,
            checkbox: true,
            sidePagination: "client", //分页方式：client客户端分页，server服务端分页（*）
            pageNumber: 1, //初始化加载第一页，默认第一页
            pageSize: 20, //每页的记录行数（*）
            pageList: [20, 40, 80], //可供选择的每页的行数（*）
            formatLoadingMessage: function () {
                return "请稍等，正在加载中。。。";
            },
            columns: [{
                field: 'id',
                title: 'ID',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'ctime',
                title: '注册时间',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'stime',
                title: '开始时间',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'location',
                title: '地点',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'state',
                title: '状态',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'name',
                title: '名称',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'unit',
                title: '单位',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center',
                formatter: function (value, row, index) {
                    return '<a href="http://' + row.unit_web + '" target="_blank" >' + value + '</a>';
                }
            }, {
                field: 'admin',
                title: '管理员',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'expert_total',
                title: '嘉宾',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_total',
                title: '报名',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_pc',
                title: 'PC报名',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_h5',
                title: 'H5报名',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_az',
                title: '安卓报名',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_import',
                title: '导入',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_del',
                title: '删除',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'hotel_total',
                title: '酒店',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'train_total',
                title: '高铁',
                align: 'center',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }]
        });
        $('#activity-table').bootstrapTable('hideColumn', 'id');
    }

    function loding() {
        $('body').addClass("modal-open");
        $('#model-div').addClass("modal-backdrop fade in");
        $('.progress').addClass("in");
        $('.progress').css('display', 'block');
        $('.progress').css('z-index', 999999);
        $('.progress').css('position', 'absolute');
        $('.progress').css('left', '20%');
        $('.progress').css('top', '49%');
    }

    function lodingDown() {
        $('body').removeClass("modal-open");
        $('.progress').css('display', 'none');
        $('.progress').css('z-index', 0);
        $('.progress').css('position', 'absolute');
        $('.progress').css('left', '20%');
        $('.progress').css('top', '49%');
        $('#model-div').removeClass("modal-backdrop fade in");
    }

    function loginout() {
        $
            .myAjax({
                url: basePath + "login/out.do",
                type: "POST",
                async: false,
                dataType: "JSON",
                success: function (result) {
                    if (result.flag) {
                        location = 'login.html';
                    } else {
                        alert(result.msg);
                    }
                },
                error: function (result) {
                    alert(result.msg);
                }
            });
    }
</script>
</html>