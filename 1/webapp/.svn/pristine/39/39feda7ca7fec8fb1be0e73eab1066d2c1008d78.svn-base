<!-- 引入项目中带有左侧菜单的公共js -->
<script src="../../../js/common/main.js"></script>
<script type="text/javascript">
$(function(){
	// 获取左侧菜单列表数据(参数为请求地址)
	initMenu('../../../js/data/administrator.json','活动管理');

	var str = '<span class="filedopt">'+
					'<input type="checkbox" class="must checkinp"/>必填'+
					'<input type="checkbox" class="optional checkinp"/>选填'+
					'<input type="checkbox" class="noneed checkinp"/>不需要'+
				'</span>'+
				'<span class="moveopt">'+
					'<img src="../../../img/moveup.png" title="上移" class="moveup"/>'+
					'<img src="../../../img/movedown.png" title="下移" class="movedown"/>'+
				'</span>';
	$(".jcondetail li").append(str);

	var noNeed = $(".noneed");
	noNeed.each(function(index){
		var flag = true;
		$(this).click(function(){
			var parentE = $(this).parent().parent();
			if(flag){
				$(parentE).attr('class','notUse');
				$(parentE).find('img').css('cursor','not-allowed');
				$(parentE).find('.fieldcon input,img').attr('disabled','disabled');
				flag = !flag;
			} else {
				$(parentE).removeClass('notUse');
				$(parentE).find('.fieldcon input,img').attr('disabled',false);
				$(parentE).find('img').css('cursor','pointer');
				flag = !flag;
			}
		});
	});

	(function(){
		var aUp = $(".moveup");
		aUp.each(function(index){
			$(this).click(function(){
				var parentE = $(this).parent().parent();
				if($(parentE).prev().is('li')){
					var prevE = $(parentE).prev();
					$(parentE).insertBefore($(prevE));
				} else {
					alert("已是第一个！");
				}
			});
		});
	})();
	(function(){
		var aUp = $(".movedown");
		aUp.each(function(index){
			$(this).click(function(){
				var parentE = $(this).parent().parent();
				if($(parentE).next().is('li')){
					var nextE = $(parentE).next();
					$(nextE).insertBefore($(parentE));
				} else {
					alert("已是最后一个！");
				}
			});
		});
	})();
});
</script>
