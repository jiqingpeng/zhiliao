.wrap{
	width:1110px;
	border:1px solid red;
	margin:0 auto;
	font-size:12px;
	.progress{
		margin-bottom:20px;
		span{
			float:left;
			vertical-align: middle;
		}
		span.txt{
			padding-right:15px;
			background: url(../../img/index/pbook.png) no-repeat 58px -9px;
			color:#2ca1e7;
		}
		span.grayicon{
			background: url(../../img/index/pbook.png) no-repeat 58px 7px;
		}
		span.graytxt{
			color:#ccc;
			background: url(../../img/index/pbook.png) no-repeat 58px 7px;
		}
		span.line{
			margin-top:6px;
			border-bottom:2px solid #83d0fe;
			padding-top:3px;
		}
		span.gray{
			border-bottom-color:#ccc;
		}
		span.nobg{
			background:0 0;
			padding-right:0;
		}
		em{
			float:left;
			width:10px;
			height:10px;
			margin:6px 6px 0 -8px;
			display: block;
			background:url(../../img/index/pbook.png) no-repeat 0 -47px;
		}
		em.gray{
			background-position: 0 -32px
		}
	}
	.content{
		.main{
			width:1110px;
			.info{
				.infoBox{
					width:660px;
					padding:0 60px 20px;
					border:1px solid #ccc;
					margin-bottom:20px;
					border-top:3px solid #2da1e7;
					.infoTit{
						padding:24px 0 22px;
						color:#666;
						font-size:16px;
						font-weight: 700;
					}
					.signin{
						padding-bottom:16px;
						border-bottom:1px solid #ddd;
						color:#333;
					}
					.row{
								padding-bottom:10px;
								height:34px;
								line-height: 34px;
								position: relative;
								input{
									margin:1px 0;
									padding:7px 10px;
								}
								.name{
									margin-left:80px;
									position: relative;
									input{
										width:210px;
									}
								}
								.selectHead{
									display: inline-block;
									
									height:32px;
									background:#fff;
									line-height: 32px;
									border:1px solid #ddd;
									border-radius: 2px 2px 0 0;
									margin-left:80px;
									.selectArrow{
										width:20px;
										height:20px;
										float: right;
										margin:6px 6px 0 0;
										background:url(../../img/index/pbook.png) 4px -140px no-repeat #e0e0e0;
									}
									.selectTxt{
										padding-left:10px;
										margin-right:30px;
										text-overflow: ellipsis;
		    							white-space: nowrap;
									}
								}
								.codeId{
									
									margin-left:20px;
									position: relative;
									input{
										width:278px;
									}	
								}
								.passPhone{
									width:418px;
									margin-left:80px;
								}
								.prompt{
									margin-left:66px;
									position: relative;
									.delIocn{
										display:inline-block;
										position: absolute;
										right:-36px;
										top:7px;
										width:20px;
										height:20px;
										background:url(../../img/index/pbook.png) -21px -27px;
									}
								}
								.tipIcon{
									width:20px;
									height:20px;
									position:absolute;
									right:8px;
									top:6px;
									background:url(../../img/index/pbook.png) no-repeat -21px -83px;
								}
								.quesIcon{
									width:20px;
									height:20px;
									position:absolute;
									right:8px;
									top:6px;
									background:url(../../img/index/pbook.png) no-repeat -21px -110px;
								}
					}
					.passengerContent{
						
						.passInfo{
							margin-bottom:30px;
							padding-top:20px;
						}
						.newBtn{
								margin-left:80px;
								a{
									display: inline-block;
									height:34px;
									line-height: 34px;
									background:#ffb44a;
									border-radius: 3px;
									text-align: center;
									font-size:16px;
									// width:2px;
								}
								a:before{
									widht:22px;
									height:22px;
									display: inline-block;
									margin-top:6px;
									marign-right:6px;
									vertical-align: top;
									background:url(../../img/index/pbook.png) no-repeat -49px -267px;
									content: "";
								}
						}
						.insurance{
							border-top:1px solid #ccc;
							padding-top:20px;
							margin-top:20px;
							.txt{
								display: inline-block;
								width:64px;
								margin-left:80px;
								
							}
							.txtr{
								margin-left:10px;
							}
						}
					}
					.contact{
						.row{
							input{
								margin-left:20px;
								width:258px;
							}
						}
					}
				}
				.check{
					border-bottom: none;
					marigin-top:20px;
					p{
						text-align: center;
					}
				}
			}
			.slide{
				width:320px;
				border:1px solid yellow;
				height:200px;
			}
		}
	}
}