<script src="../../js/jquery.min.js" type="application/javascript"></script>
<script src="../../js/mng/echarts.min.js" type="application/javascript"></script>
<script src="../../js/bootstrap.min.js" type="application/javascript"></script>
<script src="../../js/bootstrap-table.min.js" type="application/javascript"></script>
<script src="../../js/bootstrap-table-zh-CN.min.js" type="application/javascript"></script>
<script src="../../js/tableExport.js" type="application/javascript"></script>
<script src="../../js/bootstrap-table-export.js" type="application/javascript"></script>
<script src="../../js/mng/mng.js" type="application/javascript"></script>
<script src="../../js/mng/monitor-base.js" type="application/javascript"></script>
<script type="text/javascript">
    $(function () {
        loding();
        init();
        tableInit();
    });

    function init() {
        $.myAjax({
            url: basePath + "sys/monitor/activity/admin/list.do",
            success: function (result) {
                lodingDown();
                if (result.flag) {
                    $('#activity-table').bootstrapTable('load', result.data.activityAdminList);
                } else {
                    toastMsg(result.msg);
                }
            },
            error: function (result) {
                toastMsg(result.msg);
            }
        });
    }

    function tableInit() {
        $('#activity-table').bootstrapTable({
            dataType: 'json', //数据格式
            dataLocale: 'zh-US', //表格汉化
            toolbar: '#toolbar',
            striped: true, //是否显示行间隔色
            cache: false, //是否使用缓存，默认为true，所以一般情况下需要设置一下这个属性（*）
            pagination: true, //是否显示分页（*）
            sortable: true, //是否启用排序
            sortOrder: "desc", //排序方式
            search: true,
            showExport: true,
            searchFormatter: false,
            showRefresh: false, //是否显示刷新按钮
            cellStyle: true,
            checkbox: true,
            sidePagination: "client", //分页方式：client客户端分页，server服务端分页（*）
            pageNumber: 1, //初始化加载第一页，默认第一页
            pageSize: 50, //每页的记录行数（*）
            pageList: [50, 100, 200], //可供选择的每页的行数（*）
            formatLoadingMessage: function () {
                return "请稍等，正在加载中。。。";
            },
            columns: [{
                field: 'super_id',
                title: 'ID',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'name',
                title: '单位名称',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'contact_user',
                title: '单位超管',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'id',
                title: '管理员ID',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'nick',
                title: '管理员名称',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'ctime',
                title: '创建时间',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'mobile',
                title: '管理员手机号',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'cou',
                title: '关联活动',
                valign: 'middle',
                sortable: true,
                align: 'center'
//                ,
//                formatter: function (value, row, index) {
//                    return '<a href="javascript:void(0);" onclick="activityBaseMsg(\'' + row.id + '\');\" ' +
//                        'title="点击查看活动基本指标" >' + value + '</a>';
//                }
            }]
        });
        $('#activity-table').bootstrapTable('hideColumn', 'id');
        $('#activity-table').bootstrapTable('hideColumn', 'super_id');
    }

</script>
</html>