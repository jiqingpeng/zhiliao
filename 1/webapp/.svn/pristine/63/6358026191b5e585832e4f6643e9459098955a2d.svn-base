<!DOCTYPE html>
<html>
<head>
<meta http-equiv="content-type" charset="UTF-8">
<title>api-接口测试</title>
<style type="text/css">
.content {
	height: 200px;
	width: 100%;
	background-color: #eee;
}
.blk {
	display: inline-block;
}
.success, .error {
	height: 150px;
	width: 500px;
	margin: 40px 40px;
	border: 1px solid #ddd;

}

</style>
<script src="../js/jquery.min.js"></script>
<script type="text/javascript">
$(function(){

	var url = 'http://192.168.1.189:8888/' + 'admin/admin/info.do';
	var postData = '';

	$.ajax({
		url: 'http://192.168.1.189:8888/adminlogin/check.do',
		data: {phone:"13633334444",pwd:"asd"},
		dataType : 'json',

		xhrFields: {				// 解决跨域session不一致问题，上线删除，只开发使用
           withCredentials: true
        },
        crossDomain: true,

		success: function(data){
			if(data.result.flag){
				$.ajax({
					type : 'post',
					url : url,
					//data : data,
					dataType : 'json',

					xhrFields: {
			           withCredentials: true
			        },
			        crossDomain: true,

					success : function(data){
						console.log(data);
						if(data.result.flag){
							$(".success div").html(data.content);
						} else {
							$(".error div").html(data.result.msg);
						}
					},
					error : function(data) {
						alert("请求失败！！");
					}
				});
			}
		},
		error: function(){
			console.log('78676845684');
		}
	});

});
</script>
</head>
<body>
	<div class="content"></div>
	<div class="blk success">
		<label>success</label>
		<div></div>
	</div>
	<div class="blk error">
		<label>error</label>
		<div></div>
	</div>
</body>
</html>