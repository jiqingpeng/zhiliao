.detailContent{
	width: 1200px;
	/*border: 1px solid #ccc;*/
	margin: 10px auto;
}
.pictureTitle{
	height: 86px;
	background-color: #f5f5f5;
}
.pictureTitle .subName{
	font-size: 28px;
	height: 46px;
	line-height: 46px;
	font-weight: 700;
	color: #333;
	margin-left: 60px;
}
.pictureTitle .address{
	margin-left: 60px;
}
.address a{
	color: #005bb5;
}
.grogImages{
	margin-top: 10px;
}
.grogImages .grogImages-right{
	margin-left: 10px;
	float: left;
	width: 248px;
	height: 420px;
}
.grogImages .grogImages-left{
	float: left;
}

.grogImages-right .img{
	margin-top: 17px;
}
.switchover{
	width: 810px;
	height: 60px;
	border-bottom:2px solid #4c5a65;
}
.switchover li{
	float: left;
	min-width: 60px;
	line-height: 60px;
	padding: 0 20px;
	text-align: center;	
	font-weight: 700;
	font-size: 16px;
}
.switchover li a{
	color: #4c5a65;
}
.switchover li:nth-child(1){
	background-color: #4c5a65;
}
.switchover li:nth-child(1) a{
	color: #fff;
}
/*.switchover li:hover {
	background-color: #f5f5f5;
}*/
.roomDetail{
	width: 809px;
	/*height: 800px;*/
	border: 1px solid #ccc;
	border-top: 0;
	position: relative;
}
.roomTitle{
	height: 127px;
	width: 100%;
	/*border: 1px solid red;*/
	background-color: #f4f7fd;
}
.roomTitle .roomContent{
	width: 644px;
	height: 52px;
	margin: 20px 20px;
}
.roomContent img{
	width: 80px;
	height: 80px;
}
.roomContent .advanced{
	display: inline-block;
}
.roomContent .advanced .advancedRoom{
	margin-top: 10px;
	display: inline-block;
	font-size: 18px;
	font-weight: 500;
	margin-bottom: 10px;
}
.roomDetail .roomNav{
	padding-left: 107px;
	height: 36px;
	width: 703px;
	background-color: #f5f5f5;
}
.roomNav li{
	float: left;
	line-height: 36px;
	width: 90px;
}
.roomNav li:nth-child(1){
	width: 150px;
}
.roomMatter{
	padding-left: 107px;
	height: 80px;
	width: 707px;
	border-bottom: 1px dashed #ccc;
}
.roomMatter dd{
	float: left;
	line-height: 60px;
	width: 90px;
}
.roomMatter .dd{
	width: 150px;
}
.roomMatter .price{
	color: red;
	font-size: 20px;
	font-weight: 700;
}
.roomMatter .predetermine{
	margin-top: 10px;
	width: 70px;
	display: inline-block;
	height: 30px;
	line-height: 30px;
	text-align: center;
	background-color: #ff5b25;
	color: #fff;
	font-size: 16px;
	font-weight: 600;
	cursor: pointer;
}
.roomMatter .onLine{
	display: inline-block;
	width: 68px;
	height: 28px;
	line-height: 28px;
	text-align: center;
	border: 1px solid #ff5b25;
	font-size: 12px;
	color: #ff5b25;
	cursor: pointer;
}
.smallMap{
	
	width: 400px;
	height: 300px;
	border:1px solid #000;
	position: absolute;
	top: 560px;
	right: 130px;
}