		var unitprice = $("#unitprice").val();
		$(".resTolPrice, .price, .totPrice, .immedPaymentPrice").text("¥"+unitprice*roomNum*allDays);
		$(".money").html(unitprice*roomNum);

		// 增加入住客人
		var linkManNum = $(".LinkManlabel").length; 
		if(roomNum > linkManNum){
