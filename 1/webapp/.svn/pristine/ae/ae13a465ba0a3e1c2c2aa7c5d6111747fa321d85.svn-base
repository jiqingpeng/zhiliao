			$("#sdateRange").val($(".stime").html());
			$("#edateRange").val($(".etime").html());
		}
		if($(".alterData").text()=="修改日期"){
			$(".alterData").text("确 定");
		}else{
			$(".alterData").text("修改日期");
		}
	})
