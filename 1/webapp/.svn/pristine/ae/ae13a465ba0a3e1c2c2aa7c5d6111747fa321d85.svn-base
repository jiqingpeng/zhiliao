function initPage(stime,etime){ 
	$.myAjax({
		url: basePath + 'tc/hotel/getroom.do',
		data: {
			// id: '0a90d9c5867340b380c836f34621c03f',
			id: hotelData.ResId, 
			proId: hotelData.ProId,
			pduId:hotelData.ProductUniqueId, 
			stime:stime,
			etime:etime
			},
		success: function(result){
			console.log(result);
			if(result.flag){
				initCreditCard(result.data.cardType);

				initRoomNum(result.data.InventoryRemainder,result.data.OpeningSale);

				$(".roomImg").attr("src",result.data.hotelRoomImg);

				$(".finalTime").html(result.data.GuaranteeHoldTime);
				var hotelDetail = result.data.hotelDetail;
				var roomDetail = result.data.hotelRoomInfo;
				$(".ResName").html(hotelDetail.ResName);
				$(".address").html(hotelDetail.ProvName+'&nbsp;'+hotelDetail.SectionName+'&nbsp;'+hotelDetail.Address);
				$(".productType").html(roomDetail.SupPriceName);
				$(".bedType").html(roomDetail.ResProProps.床型);
				$(".roomsize").html(roomDetail.RoomSize);
				$(".floor").html(roomDetail.RoomFloor);
				if(roomDetail.BreakfastCount > 1) {
					var breakCount = '双早';
				} else if(roomDetail.BreakfastCount == 1){
					var breakCount = '单早';
				} else {
					var breakCount = '无';
				}
				$(".breakCount").html(breakCount);
				if(roomDetail.ResProProps.加床价 > 0){
					$(".addbed").html(roomDetail.ResProProps.加床价);
				} else {
					$(".addbed").html(roomDetail.ResProProps.加床);
				}
				$(".hasWindow").html(roomDetail.HasWindows==1?'有窗':'无窗');
				$(".price,.resTolPrice,.totPrice,.immedPaymentPrice").html(result.data.sale*useDate(stime,etime));
				$(".money").html(result.data.sale);
				$("#unitprice,.money").val(result.data.sale);

				// 提醒担保判断
				// $(".GuaranteeHoldTime").html(result.data.GuaranteeHoldTime);
				if(result.data.GuaranteeHoldTime.substring(0,2) <= $(".arriveTime").val()){
					$(".GuaranteeHoldTime").html($(".arriveTime").val()+':00');
					$(".warnText").show();
				}

			}
		},
		error: function(result){
			myAlert('请求失败！','slow');
		}
	});

}
// 生成房间数
function initRoomNum(data,tag){
	var roomNum = '';
	if(data>0 && data<8){
		roomNum = data;
	} else if(data>8){
		roomNum = 8;
	} else if(data == 0 && tag == 'true'){
		roomNum = 8;
	}
	var roomnumstr = '';
	for(var i=0;i<roomNum;i++){
		if(i==0){
			roomnumstr += '<span class="resRoomCon">房间数量</span>'+
						'<span class="resCouSpan active">'+(i+1)+'</span>';
		} else {
			roomnumstr += '<span class="resCouSpan">'+(i+1)+'</span>';
		}
	}
	if(roomNum < 8){
		roomnumstr += '<span class="warn">仅剩'+roomNum+'间可订</span>'
	}

	$(".reserCount").html(roomnumstr);
}

// 初始化信用卡
function initCreditCard(data){
	var aCard = $.parseJSON(data.PayMentMerchants);

	var cardList = [];
	for(var i = 0; i < aCard.length; i++){
		if(aCard[i].CardBankLogoUrl){
			cardList.push(aCard[i]);
		}
	}

	var aElem = $(".spanCard");
	var cardstr = '';
	for(var i=0;i<aElem.length;i++){
		var bank = $(aElem[i]).find(".bankName").val();
		for(var j=0;j<cardList.length;j++){
			if(cardList[j].PaymentWayName == bank){
				$(aElem[i]).find(".bankId").val(cardList[j].CreditCardBankId);
				break;
			}
		}
	}

}

// 输入验证
function checkInput(){

	// 入住信息
	var maninfo = $(".linkManName");
	maninfo.each(function(index){
		if($(this).val() == null || $(this).val() == ''){
			myAlert('请输入住客真实姓名！','slow');
			return false;
		}
	});
	if(!$(".linkManMobile").val()){
		myAlert('请输入联系电话！','slow');
		return false;
	} else if(!(/^1[34578]\d{9}$/.test($(".linkManMobile").val()))){
    	myAlert("请输入正确的联系电话！",'slow');  
    	return false;
	}
	// 信用卡信息
	var cardNumber = $(".CardNumber").val();
	if(!(/^\d{15,16}$/.test(cardNumber))) {
		myAlert("请输入15位或16位信用卡号！",'slow');  
    	return false;
	};
	var cardValueCode = $(".CardValueCode").val();
	if(!(/^\d{3}$/.test(cardValueCode))) {
		myAlert("请输入正确的CVV2验证码！",'slow');  
    	return false;
	};

	var cardValueDate = '20'+$(".CardValueDateYY").val()+'/'+$(".CardValueDateMM").val();
	if(!(/^\d{2}$/.test($(".CardValueDateYY").val())) || !(/^\d{2}$/.test($(".CardValueDateMM").val()))) {
		myAlert("请选择有效期限！",'slow');  
    	return false;
	};
	var cardUserName = $(".CardUserName").val();
	if(!cardUserName){
		myAlert("请输入持卡人姓名！",'slow');  
    	return false;
	}
	var mobile = $(".mobile").val();
	if(!(/^1[34578]\d{9}$/.test(mobile))){
    	myAlert("请输入正确的持卡人手机号！",'slow');  
    	return false;
	}
	var credentialNumber = $(".CredentialNumber").val();
	if(!(/^[1-9]\d{5}\d{2}((0[1-9])|(10|11|12))(([0-2][1-9])|10|20|30|31)\d{2}$/.test(credentialNumber))&&!(/^[1-9]\d{5}(18|19|([23]\d))\d{2}((0[1-9])|(10|11|12))(([0-2][1-9])|10|20|30|31)\d{3}[0-9Xx]$/.test(credentialNumber))){
    	myAlert("请输入正确的持卡人身份证号！",'slow');  
    	return false;
	}

	return true;
}	

// 入住天数计算
function useDate(stime,etime){
	var st = timeStamp(stime),
		et = timeStamp(etime);
	return (et-st)/(1000*24*60*60);
}

// 获取时间字符串
function add0(m){return m<10?'0'+m:m }
function format(datastr) {
	var time = new Date(datastr);
	var y = time.getFullYear();
	var m = time.getMonth()+1;
	var d = time.getDate();
	var h = time.getHours();
	var mm = time.getMinutes();
	var s = time.getSeconds();
	// return y+'-'+add0(m)+'-'+add0(d)+' '+add0(h)+':'+add0(mm)+':'+add0(s);
	return y+'-'+add0(m)+'-'+add0(d);
}
