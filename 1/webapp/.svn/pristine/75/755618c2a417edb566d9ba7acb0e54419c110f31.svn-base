<script>
	$(function(){
		//init获取页面信息和登陆状态
		url=basePath+"index/init.do"
		$.ajax({
			url: url,
			type: 'GET',
			dataType: 'json',
			xhrFields: {
       			withCredentials: true
    		},
    		crossDomain: true,
			success:function(result){
				console.log(result.data)
				//循环行业类型
					for(var i=0;i<result.data.industy.length;i++){
						$('.hlist_message').append('<li class="fl"><a href="#">'+result.data.industy[i].name+'</a></li>')
					}
				//循环行业分类信息
				for(var i=0;i<10;i++){
					result.data.activity[i].stime=result.data.activity[i].stime.substr(0,11)
					$('.actdetail').append('<li class="clearfix"><input type="hidden" value="'+result.data.activity[i].id+'"/><span class="time fl">'+result.data.activity[i].stime+'</span><a href="#" class="fl mess">'+result.data.activity[i].name+'</a><span class="place" class="fl">'+result.data.activity[i].province+'</span></li>')
				}
				//获取用户登陆状态信息
				if(result.data.user.name){
					$('#user_name').html(result.data.user.name)
				}
				
			}
		})
		//注销
		$('#login_out').click(function(){
			url=basePath+"login/out.do"
			url: url,
			$.ajax({
				url: url,
				type: 'GET',
				dataType: 'json',
				xhrFields: {
           			withCredentials: true
        		},
        		crossDomain: true,
				success:function(data){
					console.log(data.url)
					window.location.href='index.html'
				}
			})
		})
	})
</script>
