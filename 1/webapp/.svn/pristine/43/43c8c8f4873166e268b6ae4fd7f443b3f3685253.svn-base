body,ul,li {
     padding:0;
     margin:0;
     border:0;
}

body {
 font-size:14px;
 -webkit-user-select:none;
 -webkit-text-size-adjust:none;
 font-family:helvetica;
 background:#ccc;
}
	#header .mess-foot{
	padding:2px;
}
#footer {
     position:absolute;
     bottom:0; left:0;
     width:100%;
     height:48px;
     background-image:-webkit-gradient(linear, 0 0, 0 100%, color-stop(0, #999), color-stop(0.02, #666), color-stop(1, #222));
     background-image:-moz-linear-gradient(top, #999, #666 2%, #222);
     background-image:-o-linear-gradient(top, #999, #666 2%, #222);
     padding:0;
     border-top:1px solid #444;
}
	
#wrapper {
 z-index:1;
 bottom:1px; 
 left:0;
 width:100%;
 overflow:auto;
}

#scroller {
 position:relative;
 -webkit-tap-highlight-color:rgba(0,0,0,0);
float:left;
 width:100%;
 padding:0;
}

#pullDown, #pullUp {
    background:#fff;
    height:40px;
    line-height:40px;
    padding:5px 10px;
    border-bottom:1px solid #ccc;
    font-weight:bold;
    font-size:14px;
    color:#ccc;
    text-align: center;
}
.floor{
	width:100%;
	padding:10px;
	margin-bottom:1px;
	background:#fff;
}
.floor .talk-txt{
	width:86%;
}
.floor .mess-foot{
	margin-left:14%;
}
.child-box .mess-foot{
	margin-left:0;
}
.mess-head .mess-tit{
	width:86%;
}
.mess-head .mess-icon{
	width:30px;
	padding:0;
}
img{
	width:30px;
	height:30px;
	border-radius: 50%;
	background:#f0f0f0;
}
.mess-head .mess-tit .mess-time{
	
}
.tit-cont{
	padding-top:10px;
	text-align:center;
	font-size:16px;
	font-weight: bold;
	color:#999;
}

.mess-foot .item{
	padding:0;
	float:right;
}
.mess-foot .item img{
	/*border:1px solid red;*/
	display: inline-block;
	width:18px;
	height:20px;
	vertical-align: top;
}

.mess-talk{
	width:100%;
	height:40px;
	position:fixed;
	left:0;
	bottom:1px;
	display:none;
	z-index:5;
	background:#fff;
}
.mess-talk .mess-txt{
	width:100%;
}
.mess-talk .mess-txt img{
	width:30px;
	height:30px;
	padding:0;
}
.mess-talk .mess-txt input{
	height:40px;
	padding:0;
}
.mess-foot .mess-time{
	padding:0;
}
.mess-talk-group{
	background:#ccc;
	border-radius:4px;
}
.mess-talk-group .page-more{
	margin-left:2%;
	margin-top:5px;
}
.mess-talk-group img{
	width:30px;
	height:30px;
	border-radius: 50%;
}
.mess-talk-group .talk-txt{
	width:86%;
	/*overflow:hidden;*/
}
.talk-btn{
	display: inline-block;
	width:64px;
	height:40px;
	line-height: 40px;
	background:#f60;
	color:#fff;
	text-align:center;
	border-radius:2px;
	float:right;
}
.mess-child .second{
	display:none;
}
.mess-child .child-floorw{
	width:100%;
}
.child-box{
	padding:5px;
}
.mess-child .child-floorw .talk-txt{
	width:100%;
	word-wrap:break-word ;
}
.reply{
	display: inline-block;
	width:100%;
	text-align: right;
}
.f60{
	color:#f60;
}
@media (min-width: 750px) and (max-width: 970px){
			.floor img{
				width:40px;
				height:40px;
				margin-left:6%;
			}
			#thelist{
				margin-left:8%;
				margin-right:14%;
			}
			#wrapper{
				top:65px;
				background:#fff;
				position:absolute;
			}
		}
		@media(min-width: 970px){
			.talk-txt,.num,.page-more{
				font-size:14px;
			}
			.tit-cont{
				font-size:20px;
			}
			#wrapper{
				top:65px;
				background:#fff;
				position:absolute;
			}
			.header{
				width:100%;
				margin:0 auto;
			}
			.header .message-box{
				width:80%;
				margin:0 auto;
			}
			#thelist{
				/*width:80%;
				margin:0 auto;*/
				margin-left:8%;
				margin-right:14%;
			}
			.floor img{
				width:50px;
				height:50px;
				margin-left:6%;
			}
			.child-box img{
				width:30px;
				height:30px;
				margin-left:0;
			}
			.talk-txt{
				width:88%;
			}
			
			.child-box .comment{
				width:12%;
			}
			.mess-talk{
				padding:0 10%;
				/*height:50px;*/
			}
			.mess-talk .mess-txt input{
				/*height:50px;*/
			}
		}