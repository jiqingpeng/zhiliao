<script type="text/javascript">

	$(function(){
		document.onmousemove = function(e){
			// console.log(document.body.scrollTop);
			if(document.body.scrollTop>568){
				$(".switchover").css("position","fixed");
				$(".switchover").css("left", "200");
				$(".switchover").css("top", "0");
				$(".switchover").css("z-index", "100");
				$(".switchover").css("backgroundColor", "#fff");
			}
			if(document.body.scrollTop<568){
				$(".switchover").css("position","static");
			}
		};
		$(".switchover li").on("click", function(){
			$(this).css("backgroundColor", "#4c5a65").siblings().css("backgroundColor", "#fff");
			$(this).children().css("color", "#fff").parents().siblings().children().css("color", "#000");
		});
	})
</script>
<script type="text/javascript">
	 //创建和初始化地图函数：
    function initMap(){
      createMap();//创建地图
      setMapEvent();//设置地图事件
    }
    function createMap(){ 
      map = new BMap.Map("map"); 
      map.setCurrentCity("北京"); 
      map.centerAndZoom(new BMap.Point(116.394675,39.917545),11);
      marker = new BMap.Marker(new BMap.Point(116.394675, 39.917545));
      map.addOverlay(marker);
    }
    function setMapEvent(){
      map.enableScrollWheelZoom();
      map.enableKeyboard();
      map.enableDragging();
      map.enableDoubleClickZoom()
    }
    function addClickHandler(target,window){
      target.addEventListener("click",function(){
        target.openInfoWindow(window);
      });
    }
    var map;
      initMap();
</script>
