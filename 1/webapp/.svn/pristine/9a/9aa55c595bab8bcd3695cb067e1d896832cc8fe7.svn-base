@charset "utf-8";

/* reset */
body,h1,h2,h3,h4,h5,p,dd,ul,form,input,textarea,th,td,select,button { margin:0; padding:0; }
em { font-style:normal; }
li { list-style:none; }
a { text-decoration:none; }
img { border:none; vertical-align:top; }
table { border-collapse:collapse; }
input,textarea { outline:none; }
textarea { resize:none; overflow:auto; }
body { font-size:14px; font-family:"SimSun","宋体","Arial Narrow",HELVETICA; background-color: #e8eaf0; }
/* end reset */

body {
	min-width: 980px;
	min-height: 670px;
}
.box{
	top: 156px;
}
.user-nav{
    font-size:18px;
	color:#333;
	height: 45px;
	line-height: 45px;
	border-bottom:1px #c9924b solid; 
	margin-bottom: 20px;
	padding-left: 14px;
}

.red {
	color: red;
}
.prejoincon {
	background: #fff;
	width: 1100px;
	min-height: 600px;
	margin: 20px auto;
	padding-top: 1px;
}

.joincon {
	background: #fff;
	/*width: 1100px;*/
	min-height: 800px;
	padding-top: 1px;
}
.activname {
	height: 56px;
	font-size: 22px;
	line-height: 56px;
	text-align: center;
	margin-top: 14px;
	font-weight: bold;
	color: #333;
}
.jcon {
	position: relative;
	width: 960px;
	min-height: 80px;
	margin: 0 70px 0 70px;
}
.jconheader {
	height: 38px;
	border-bottom: 1px solid #ccc;
	margin-bottom: 10px;
}


.jcontil {
	display: inline-block;
	width: 120px;
	height: 36px;
	line-height: 36px;
	vertical-align: top;
	text-align: center;
	border-bottom: 2px solid #2bc6fb;
	position: relative;
	top: 2px;
}
.jconnum {
	display: inline-block;
	height: 21px;
	width: 21px;
	line-height: 21px;
	background: #ff8400;
	color: #fff;
	margin-right: 10px;
	border-radius: 11px 11px;
}

/* 字段配置 */
.person .jconeditdetail {
	max-height: 485px;
	overflow: hidden;
}
.jconeditdetail li {
	height: 32px;
	line-height: 32px;
	position: relative;
}
.type {
	display: inline-block;
	height: 36px;
	width: 140px;
	margin-left: 24px;
	text-align: right;
}

/* 字段配置-end*/

/* 字段预览 */
.jcondetail {
	margin-top: 24px;
}
.jcondetail li {
	height: 46px;
	line-height: 46px;
	position: relative;
}
.til {
	display: inline-block;
	width: 108px;
	height: 38px;
	line-height: 38px;
	text-align: right;
}
/*.red {
	color: red;
	margin-right: 2px;
}*/
.inp {
	height: 36px;
	padding-left: 10px;
	width: 342px;
	border: 1px solid #ddd;
	border-radius: 3px 3px;
	margin-left: 12px;
}

.radioinp {
	height: 18px;
	width: 18px;
	margin: 0 14px 0 6px !important;
	position: relative;
	top: 6px;
	display: inline-block;
}

.notUse { /* 不需要的样式 */
	background: #ddd;
}
.notUse label { 
	color: #999;
}

/* 字段预览-end*/

/* 右侧操作部分样式 */
.filedopt {
	margin: 0 12px;
	position: absolute;
	right: 360px;
}
.checkinp {
	margin: 0 2px 0 30px !important;
	
}
.moveopt {
	position: absolute;
	right: 220px;
}
.moveopt img {
	height: 22px;
	width: 22px;
	cursor: pointer;
	position: relative;
	top: 5px;
	margin-right: 20px;
}

/* 底部按钮 */
.joinbtn {
	height: 68px;
	line-height: 68px;

	border-top: 1px solid #ccc;
	text-align: center;
}

.preview {
	margin-left: 80px;
}

.morefiled {
	width: 720px;
	min-height: 10px;
	/*line-height: 50px;*/
	/*color: #ff8400;*/
	/*text-align: right;*/
	/*cursor: pointer;*/
}

.addfieldbtn {
	/*margin: 10px 0 0  76px;*/
	/*margin-left: 780px;*/
	/*margin-top: -20px;*/
