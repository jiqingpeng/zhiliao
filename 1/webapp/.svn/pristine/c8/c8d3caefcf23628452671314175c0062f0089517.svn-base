.grogshopDetail{
	width: 840px;
	float: left;
}
.grogshopDetail .grogshopTitle{
	height: 42px;
	background-color: #f7f7f7;
	border: 1px solid #ccc;
}
.grogshopTitle li{
	width: 79px;
	line-height: 42px;
	text-align: center;
	float: left;
	border-right: 1px solid #ccc;
}
.grogshopContent{
	position: relative;
	width: 840px;
	height: 165px;
	/*border: 1px solid #ccc;*/
	border-bottom: 1px solid #ccc;
	margin-top: 20px;
}
.grogshopContent :hover{
	background-color: #f7f7f7;
}
.grogshopContent .grogConBox{
	height: 135px;
	margin-top: 14px; 
}
.grogshopContent .imagesBox{
	float: left;
}
.grogshopContent .grogshopImg{
	margin-left: 10px;
}
.grogshopContent .bigGrogshopImg{
	width: 300px;
	height: 240px;
	border: 1px solid #000;
	position: absolute;
	top: 0px;
	left: 185px;
	z-index: 1000;
}
.grogConBox .grogConIntro{
	margin-left: 10px;
	width: 280px;
	height: 131px;
	float: left;
}
.grogConIntro .num{
	display: inline-block;
	width: 23px;
	height: 32px;
	line-height: 25px;
	text-align: center;
	background: url("../../img/index/icon-num.png") no-repeat;
}
.grogConIntro h4{
	font-weight: 700;
	font-size: 18px;
	color: #000;
	display: inline-block;
	cursor: pointer;
}

.grogConIntro a{
	margin-top: 10px;
	display: inline-block;
	margin-left: 7px;
}
.grogConIntro span{
	color: #ccc;
	margin-left: 7px;
}
.grogConIntro .business{
	color: #fff;
	display: inline-block;
	width:	60px;
	height: 20px; 
	background-color: #64bbff;
	text-align: center;
	margin-left: 7px;
}
.grogConIntro .bottomSpan{
	display: inline-block;
	margin-top: 10px;
}
.grogshopprice{
	width: 340px;
	border-right: 1px solid #ccc;
	float: left;
	margin-left: 10px;
	height: 130px;
	color: #000;
	overflow: hidden;
}
.grogshopprice .grogRiceLeft{
	float: left;
	width: 180px;
	height: 100%;
	border-left: 1px solid #ccc;
	border-right: 1px solid #ccc;
}
.grogshopprice span{
	display: inline-block;
	margin-left: 10px;
	margin-top: 5px;
}
.grogshopprice .grade{
	color: #ff7800;
	font-size: 18px;
	font-weight: 700;
	margin-top: 25px;
}
.grogshopprice .comment{
	color: #ff7800;
}
.grogshopprice .reserve{
	color: #50b400;

}
.grogshopprice .grogRiceRight{
	float: right;
	color: #ff7800;
}
.grogRiceRight span{
	margin-top: 30px;
	font-size: 30px;
	font-weight: 700;
	margin-right: 20px;
}
.grogRiceRight button{
	width: 120px;
	height: 40px;
	background-color: #ff7800;
	border: 1px solid #ff7800;
	color: #fff;
	font-size: 14px;
	margin-right: 20px;
	margin-top: 15px;
	cursor: pointer;
}
.grogRiceRight:hover button {
	background-color: #ff7800;
}
.grogshopMap{
	position: absolute;
	width: 300px;
	height: 400px;
	top: 0;
	right: 10px;
}
.grogshopMap .checkMap{
	width: 120px;
	height: 44px;
	cursor: pointer;
	font-weight: 600;
	line-height: 44px;
	color: #fff;
	text-align: center;
	background-color: #50b400;
}
.grogshopMap .detailMap{
	height: 355px;
	width: 300px;
	font-size: 12px;
}
