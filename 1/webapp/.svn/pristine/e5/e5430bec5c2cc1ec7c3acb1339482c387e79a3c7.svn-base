<style>
    .m-loca{
        margin-left:20px;
        margin-top:44px;
    }   
   .ml70{
            margin-left:70px;
        }
    .ml60{
        margin-left:60px;
    }
    .mt20{
      margin-top:20px;
    }
    .mt40{
      margin-top:40px;
    }
    #preview_wrapper
    {
        display: inline-block;
        width: 300px;
        height: 200px;
        background-color: #CCC;
        margin-left:14px;
    }
    
    #preview_fake
    {
        /* 该对象用户在IE下显示预览图片 */
        filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod=scale);
    }
    #preview_size_fake
    {
        /* 该对象只用来在IE下获得图片的原始尺寸，无其它用途 */
        filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod=image);
        visibility: hidden;
    }
    #preview
    {
        /* 该对象用户在FF下显示预览图片 */
        width: 300px;
        height: 200px;
    }
    .btn-location{
  		width:300px;
  		display: block;
  		margin:0 auto;
  		margin-bottom:20px;
  	}
    .cred{
      color:red;
    }
</style>
</head>
<body>
