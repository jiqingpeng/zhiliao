div.datepicker {
    position: relative;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 12px;
    width: 196px;
    height: 147px;
    position: absolute;
    cursor: default;
    top: 0;
    left: 0;
    display: none;
}

.datepickerContainer {
    background: #eee;
    position: absolute;
    top: 4px;
    left: 4px;
    border-top: 2px solid #f60;
    border-left: 2px solid #f60;
    border-radius: 4px 4px 0 0;
    box-shadow: 3px 3px #f60;
}

.datepickerBorderT {
    position: absolute;
    left: 4px;
    top: 0;
    right: 4px;
    height: 4px;
    /*background-color: #fe6600;*/
    /*background: url(../../../img/mng/datepicker/datepicker_t.png);*/
}

