.switchover{
	width: 810px;
	height: 60px;
	border-bottom:2px solid #4c5a65;
}
.switchover li{
	float: left;
	min-width: 60px;
	line-height: 60px;
	padding: 0 20px;
	text-align: center;	
	font-weight: 700;
	font-size: 16px;
}
.switchover li a{
	color: #4c5a65;
}
.switchover li:nth-child(1){
	background-color: #4c5a65;
}
.switchover li:nth-child(1) a{
	color: #fff;
}
