/* 页面公共部分样式 */
li a {
	color: #122e67;
}
li a:hover {
	color: #ff8400;
}
li a:visited {
	color: #122e67;
}
.mops-con {
	
}
.inlblk {
	display: inline-block;
}
.con-nav {
    font-size:18px;
	color:#333;
	height: 45px;
	line-height: 45px;
	border-bottom:1px #c9924b solid; 
	margin-bottom: 20px;
	padding-left: 14px;
}
.con-box {
	margin-left: 3%;
}
/* 页面公共部分样式 --END */


/* 活动列表部分 */
.activityList {
	width: 100%;
	height: 98%;
	min-height: 560px;
}
.activityList li {
	white-space:nowrap;
	overflow:hidden; 
	text-overflow:ellipsis;
	list-style: none;
	height: 38px;
	line-height: 38px;
}
.actstime, .place {
	font-size: 14px;
	margin-right: 12px;
	display: inline-block;
	height: 22px;
	padding-right: 12px;
	border-right: 2px solid #ccc;
	line-height: 22px;
	color: #122e67;
}
/* 活动列表部分-end */

