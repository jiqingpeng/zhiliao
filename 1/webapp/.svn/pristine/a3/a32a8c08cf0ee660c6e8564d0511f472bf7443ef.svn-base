		};
		//短信验证
		if (prov == '' || prov == null) {
			myAlert('短信验证码不能为空','slow');
			return false;
		}else{
			if(!(/^\d{6}$/.test(prov))){
				myAlert('请输入6位数字短信验证码','slow');
				return false;
			};
		};
		return true;
	};
