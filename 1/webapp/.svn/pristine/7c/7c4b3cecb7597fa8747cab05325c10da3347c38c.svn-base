/*调用类名实现相应的效果*/
/*全屏缩放效果*/
@keyframes fullScreenZoom{
	0%{
		transform: scale(0.1,0.1);
		opacity: 0;
	}
	100%{
		transform: scale(1.2,1.2);
		opacity: 1;
	}
}
@-webkit-keyframes fullScreenZoom{
	0%{
		-webkit-transform: scale(0.1,0.1);
		opacity: 0;
	}
	100%{
		-webkit-transform: scale(1.2,1.2);
		opacity: 1;
	}
}
@-moz-keyframes fullScreenZoom{
	0%{
		-moz-transform: scale(0.1,0.1);
		opacity: 0;
	}
	100%{
		-moz-transform: scale(1.2,1.2);
		opacity: 1;
	}
}
.fullScreenZoom{
	animation: fullScreenZoom 2s;
	-webkit-animation: fullScreenZoom 2s;
	-moz-animation: fullScreenZoom 2s;
	animation-fill-mode: forwards;
}

/*文字放大效果延时0.5s*/
@keyframes characterZoom{
	0%{
		transform: scale(0.1,0.1);
		opacity: 0;
	}
	25%{
		transform: scale(0.7,0.7);
		opacity: 1;
	}
	100%{
		transform: scale(1,1);
		opacity: 1;
	}
}
@-webkit-keyframes characterZoom{
	0%{
		-webkit-transform: scale(0.1,0.1);
		opacity: 0;
	}
	25%{
		-webkit-transform: scale(0.7,0.7);
		opacity: 1;
	}
	100%{
		-webkit-transform: scale(1,1);
		opacity: 1;
	}
}
@-moz-keyframes characterZoom{
	0%{
		-moz-transform: scale(0.1,0.1);
		opacity: 0;
	}
	25%{
		-moz-transform: scale(0.7,0.7);
		opacity: 1;
	}
	100%{
		-moz-transform: scale(1,1);
		opacity: 1;
	}
}
.characterZoom {
	animation: characterZoom 2s 0.5s;
	-webkit-animation: characterZoom 2s 0.5s;
	-moz-animation: characterZoom 2s 0.5s;
	animation-fill-mode: forwards;
}

