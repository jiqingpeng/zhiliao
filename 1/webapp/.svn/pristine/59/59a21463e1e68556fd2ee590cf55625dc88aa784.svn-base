<script>
	$(function(){
		//init获取页面信息
		var url=basePath+"index/init.do"
		$.ajax({
			url: url,
			type: 'POST',
			dataType: 'json',
			xhrFields: {
       			withCredentials: true
    		},
    		crossDomain: true,
			success:function(result){
				console.log(result)
				//循环行业类型
				for(var i=0;i<9;i++){
					$('.nav').append('<li class="fl"><a href="#">'+result.data.industy[i].name+'</a></li>')
				};
				//限制条数
				var typeOne = 0;
				var typeTwo = 0;
				var typeThree = 0;
				var typeFour = 0;
				//分类信息
				for(var i=0;i<result.data.activity.length;i++){
					//截取时间
					var stime=result.data.activity[i].stime.substr(0,11);
					//填充数据
					var str='<li class="clearfix"><span class="fl dot"></span><input class="fl" type="hidden" value="'+result.data.activity[i].id+'"/><span class="time fl">'+stime+'</span><span class="city fl"><i>'+result.data.activity[i].city+'</i></span><span class="on_mess fl" >'+result.data.activity[i].name+'</span></p></li>';
					//会议
					if(result.data.activity[i].type=='1'){
						if(typeOne>9){
							continue;
						} else {
							typeOne++;
							$('.mess').append(str);
						};
					};
					//会展
					if(result.data.activity[i].type=='2'){
						if(typeTwo>9){
							continue;
						} else {
							typeTwo++;
							$('.messShow').append(str);
						};
					};
					//培训
					if(result.data.activity[i].type=='3'){
						if(typeThree>9){
							continue;
						} else {
							typeThree++;
							$('.training').append(str);
						};
					};
					//大赛
					if(result.data.activity[i].type=='4'){
						if(typeFour>9){
							continue;
						} else {
							typeFour++;
							$('.competition').append(str);
						};
					};
				};
				//登陆状态
				if(result.data.user){
					$('.log_group').addClass('active');
					$('.log_group2').removeClass('active');
					$('#user_name').html(result.data.user.name);
				};
			}
		});
		//链接跳活动详情
		$(document).on('click','.on_mess',function(){
			window.location.href='detail.html?location='+$(this).prev().prev().prev().val();
		});
		//登陆
		$('#login').click(function(){
			url=basePath+"user/login/check.do";
			$.ajax({
				url: url,
				data: {
					mobile: $('.user').val(),
					pwd:$('.password').val() 
					// phone: '13633334444',
					// pwd:'asd' 
				},
				type: 'POST',
				dataType: 'json',
				xhrFields: {
           			withCredentials: true
        		},
        		crossDomain: true,
				success:function(result){
					console.log(result)
					if(result.flag){
						window.location.href='../user/activitylist.html'
					}else{
						$('.login_message').css('display','block')
						$('.login_message').html(result.msg)
					};
				}
			});
		});
		//注销
		$('#login_out').click(function(){
			url=basePath+"login/out.do";
			$.ajax({
				url: url,
				type: 'GET',
				dataType: 'json',
				xhrFields: {
           			withCredentials: true
        		},
        		crossDomain: true,
				success:function(result){
					console.log(result)
					if(result.flag){
						window.location.href='index.html'
					};
				}
			});
		});
	});
</script>
