.wrap .listBody .tickImg {
    width: 50px;
    height: 50px;
   
}
.wrap .listBody .tickImg img{
    width:100%;
    height:100%;
}
.wrap .listBody .station {
    width: 200px;
    margin-left: 10px
}
.wrap .listBody .station .trainNo{
    font-size:20px;
    font-weight: bold;
}
.wrap .listBody .station .outOrderNo{
   color:#999;
   margin-top:5px;
}
.wrap .listBody .price {
    width: 90px;
    margin-left: 80px;
    font-size: 16px;
    color: #f60;
}
.wrap .listBody .price .icon{
    font-size:12px;
}
.wrap .listBody .price .text{
    font-size:20px;
    margin-left:-10px;
}
.wrap .listBody .time {
    width: 150px;
    font-size: 14px;
    margin-left:86px;
}

.wrap .listBody .tickMessage {
    width:80px;
    text-align: center;
    margin-left: 6px;
    // border:1px solid red;
}

.wrap .listBody .look {
    margin-left: 52px
}
