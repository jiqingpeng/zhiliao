<script>
	$(function(){
		//init获取页面信息和登陆状态
		var url=basePath+"index/search.do";
		var type=Request('type');
		var trade=Request('trade');
		console.log(type);
		console.log(trade);
		//分页数据
		var pageData=[];
		//分页
		function pageShow(){  
		    var Count = pageData.length;//记录条数  
		    var PageSize=10;//设置每页示数目  
		    var PageCount=Math.ceil(Count/PageSize);//计算总页数  
		    var currentPage =1;//当前页，默认为1。
		    var selectPage=currentPage;
		    //默认显示首页
		    for(var i=(currentPage-1)*PageSize;i<PageSize*currentPage;i++){    
		        $('.actdetail').append(pageData[i]);  
		    };
		    //显示页数
		    for(var i=1;i<=PageCount;i++){
		    	$('.pageList').append('<li class="fl"><a href="#">'+i+'</a></li>');
		    };
		    //条数
		    $('.pageCount').html('共&nbsp'+Count+'&nbsp条')
		    $('.pageList li a').eq(currentPage-1).addClass('org'); 
		    //显示选择页的内容  
		    $('.pageList li a').on('click',function(){  
		        selectPage=($(this).parent().index()+1);
		        page();  
		    });
		    //每页
		    function page(){
		    	$('.pageList li a').removeClass('org');  
		        $('.pageList li a').eq(selectPage-1).addClass('org'); 
		        $('.actdetail').html('');
		        for(i=(selectPage-1)*PageSize;i<PageSize*selectPage;i++){  
		            $('.actdetail').append(pageData[i]);  
		        };
		    }
		    //上一页
		    $('.prev').click(function(){
		    	selectPage--;
		    	if(selectPage<1){
		    		myAlert('已到首页');
		    		firstpage();
		    	}
		    	page();
		    });
		    //下一页 
		    $('.next').click(function(){
		    	selectPage++;
		    	if(selectPage>$('.pageList li').length){
		    		myAlert('已到尾页');
		    		lastpage();
		    	};
		    	page();
		    });
		    //首页
		    $('.first_page').click(firstpage)
		    function firstpage(){
			        selectPage=1;
			        page();
			}
		    //尾页
		    $('.last_page').click(lastpage)
		    function lastpage(){
		        selectPage=$('.pageList li').length;
		        page();
		    }
		    //键盘选择页面
		    $(document).keypress(function(e) {  
					// 回车键事件  
			        if(e.which == 13) {
			        	keyup()
			        };	  
			});
	    	function keyup(){
	        	if($('.kup').val()>PageCount){
	        		myAlert('超出列表页数');

	        	}else if($('.kup').val()<1){
	        		myAlert('最小页数为1');
	        	}else{
	        		$('.actdetail').html('');
		        	selectPage=currentPage=$('.kup').val();
					console.log(selectPage);
					page();
				}; 
			};
			//点击跳转
			$('#jump').on('click',keyup)
		};
		//遍历数据
		if(trade){
			$.myAjax({
				url: url,
				data:{'type':type},
				type: 'GET',
				success:function(result){
					console.log(result)
					if(result.flag){
						//用户登陆状态信息
						if(result.status.url){
							$('#user_name').html(result.status.nick);
							$('#user_center').html('用户中心');
							$('#login_out').html('注销');
							$('#user_center').attr('href',basePath+result.status.url);
						};
						//行业类型
						for(var i=0;i<result.data.industy.length;i++){
							if(i+1==trade){
								$('.hlist_message').append('<li class="fl"><input type="hidden" value="'+result.data.industy[i].value+'"><a class="on_cat org" href="javascript:;">'+result.data.industy[i].name+'</a></li>');
							}else{
								$('.hlist_message').append('<li class="fl"><input type="hidden" value="'+result.data.industy[i].value+'"><a class="on_cat" href="javascript:;">'+result.data.industy[i].name+'</a></li>');
							};
						};
						//行业分类信息
						for(var i=0;i<result.data.activity.length;i++){
							var stime= result.data.activity[i].stime.substr(0,11); 
						    var str='<li class="clearfix"><input class="fl" type="hidden" value="'+result.data.activity[i].id+'"/><span class="time fl">'+stime+'</span><span class="place fl">'+result.data.activity[i].city+'</span><a href="javascript:;" class="fl mess on_mess" title="'+result.data.activity[i].name+'">'+result.data.activity[i].name+'</a></li>';
						    pageData.push(str);  
						};
						// console.log(pageData.length)
						//分页
						pageShow();
						
					}else{
						myAlert(result.msg);
					}
				}
			});
		}else{
			$.myAjax({
				url: url,
				data:{'type':type,'trade':trade},
				type: 'GET',
				success:function(result){
					if(result.flag){
						//行业类型
						for(var i=0;i<result.data.industy.length;i++){
							
							if(i+1==trade){
								$('.hlist_message').append('<li class="fl"><input type="hidden" value="'+result.data.industy[i].value+'"><a class="on_cat org" href="#">'+result.data.industy[i].name+'</a></li>');
							}else{
								$('.hlist_message').append('<li class="fl"><input type="hidden" value="'+result.data.industy[i].value+'"><a class="on_cat" href="#">'+result.data.industy[i].name+'</a></li>');
							};
						};
						//行业分类信息
						for(var i=0;i<result.data.activity.length;i++){
							var stime= result.data.activity[i].stime.substr(0,11); 
						    var str='<li class="clearfix"><input class="fl" type="hidden" value="'+result.data.activity[i].id+'"/><span class="time fl">'+stime+'</span><span class="place fl">'+result.data.activity[i].city+'</span><a href="#" class="fl mess on_mess">'+result.data.activity[i].name+'</a></li>';
						    pageData.push(str);  
						};
						// console.log(pageData.length)
						//分页
						pageShow();
						//用户登陆状态信息
						if(result.data.user){
							$('#user_name').html(result.data.user.name);
							$('#user_center').html('用户中心');
							$('#login_out').html('注销');
						};
					}else{
						myAlert(result.msg);
					};
				}
			});
		};
		//跳活动详情
		$(document).on('click','.on_mess',function(){
			$(this).attr('href','detail.html?location='+$(this).prev().prev().prev().val());
		});
		//行情分类
		$(document).on('click','.on_cat',function(){
			$('.on_cat').removeClass('org');
			$(this).addClass('org');
			trade=$(this).prev().val();
			$('.actdetail').html('');
			pageData=[];
			console.log(type);
			console.log(trade);
			$.myAjax({
				url: url,
				data:{'type':type,'trade':trade},
				type: 'GET',
				success:function(result){
					if(result.flag){
						//行业分类信息
						console.log(result)
						$('.pageList').html('');
						for(var i=0;i<result.data.activity.length;i++){
							var stime= result.data.activity[i].stime.substr(0,11); 
						    var str='<li class="clearfix"><input class="fl" type="hidden" value="'+result.data.activity[i].id+'"/><span class="time fl">'+stime+'</span><span class="place fl">'+result.data.activity[i].city+'</span><a href="#" class="fl mess on_mess" title="'+result.data.activity[i].name+'">'+result.data.activity[i].name+'</a></li>';
						    pageData.push(str);  
						};
						//分页
						pageShow();
					}else{
						$('.pageCount').html('共&nbsp0&nbsp条');
					}
				},
				error:function(){
					myAlert('请求失败');
				}
			});
		});
		//注销
		loginOut('#login_out');
		//页面链接
		$('.cation').click(function(){
			$(this).attr('href',window.location);
		});
	})
</script>
