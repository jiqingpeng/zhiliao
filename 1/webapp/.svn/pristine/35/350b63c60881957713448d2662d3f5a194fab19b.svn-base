<script type="text/javascript">
    var avtData = [];
    var selectVal = "";
    var myChart = echarts.init(document.getElementById('activity-graph'));
    var isInitOptions = false;
    var fullData = [];
    $(function () {
        loding();
        init();
        tableInit();
    });

    function init() {
        $.myAjax({
            url: basePath + "monitor/activity/list.do",
            type: "POST",
            async: false,
            dataType: "JSON",
            success: function (result) {
                lodingDown();
                if (result.flag) {
                    avtData = result.data.activityList;
                    $('#activity-table').bootstrapTable('load', result.data.activityList);
                } else {
                    alert(result.msg);
                }
            },
            error: function (result) {
                alert(result.msg);
            }
        });
        $('#activity-graph-div').show();
        $("#activity-graph").html('');
        myChart.dispose();
        myChart = echarts.init(document.getElementById('activity-graph'));
    }

    function tableInit() {
        $('#activity-graph-div').hide();
        $('#activity-table').bootstrapTable({
            dataType: 'json', //数据格式
            dataLocale: 'zh-US', //表格汉化
            toolbar: '#toolbar',
            striped: true, //是否显示行间隔色
            cache: false, //是否使用缓存，默认为true，所以一般情况下需要设置一下这个属性（*）
            pagination: true, //是否显示分页（*）
            sortable: true, //是否启用排序
            sortOrder: "desc", //排序方式
            search: true,
            showExport: true,
            searchFormatter: false,
            showRefresh: false, //是否显示刷新按钮
            cellStyle: true,
            checkbox: true,
            sidePagination: "client", //分页方式：client客户端分页，server服务端分页（*）
            pageNumber: 1, //初始化加载第一页，默认第一页
            pageSize: 20, //每页的记录行数（*）
            pageList: [20, 40, 80], //可供选择的每页的行数（*）
            formatLoadingMessage: function () {
                return "请稍等，正在加载中。。。";
            },
            columns: [{
                field: 'id',
                title: 'ID',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'ctime',
                title: '注册时间',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'stime',
                title: '开始时间',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'location',
                title: '地点',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'state',
                title: '状态',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'name',
                title: '名称',
                valign: 'middle',
                sortable: true,
                align: 'center',
                formatter: function (value, row, index) {
                    return '<a href="javascript:void(0);" onclick="activityBaseMsg(\'' + row.id + '\');\" ' +
                        'title="点击查看活动基本指标" >' + value + '</a>';
                }
            }, {
                field: 'unit',
                title: '单位',
                valign: 'middle',
                sortable: true,
                align: 'center',
                formatter: function (value, row, index) {
                    return '<a href="http://' + row.unit_web + '" target="_blank" title="点击前往官网" >' + value + '</a>';
                }
            }, {
                field: 'admin',
                title: '管理员',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'expert_total',
                title: '嘉宾',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_total',
                title: '报名',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_pc',
                title: 'PC报名',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_h5',
                title: 'H5报名',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_az',
                title: '安卓报名',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_import',
                title: '导入',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
                field: 'attend_del',
                title: '删除',
                valign: 'middle',
                sortable: true,
                align: 'center'
            }, {
