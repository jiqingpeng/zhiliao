<!doctype html>
<html lang="en">
<head>
<meta charset="UTF-8">
<title>活动基本信息</title>
<!-- webicon.ico为页面icon -->
<link rel="shortcut icon" href="../../../img/webicon.ico" type="image/x-icon"/>
<!-- 引入项目依赖框架css : bootstrap.min.css -->
<link href="../../../css/common/bootstrap.min.css" rel="stylesheet" />
<!-- 引入项目公共样式 : style.css -->
<link href="../../../css/style.css" rel="stylesheet"/>
<!-- 引入项目中带有左侧菜单栏的公共样式 : main.css -->
<link href="../../../css/main.css" rel="stylesheet"/>
</head>
<body>
<!-- 引入页头 -->
<script type="text/javascript" src="../../../js/common/header.js"></script>
<!-- 以下为内容部分，内容部分分为导航头和主体内容 -->
<div class="activitylist mops-con"><!-- 内容部分的class : mops-con (必须) -->
	<div class="con-nav">
		<a href="message.html">管理中心</a>
		<a href="">-></a>
		<a href="message.html">活动管理</a>
		<a href="">-></a>
		<a href="message.html">基本信息</a>
	</div><!-- 内容部分导航头的class : con-nav (必须) -->
	<div class="con-box" id="box"><!-- 内容部分主体内容的class : con-box (必须) -->
		<!-- 以下为每个页面自己编写的部分 -->
		<form id="detailform" class="form-horizontal">
			<input type="reset" style="display:none;"/>
			<div class="form-group">
			    <label for="" class="col-sm-2 control-label"><span style="color: red">* </span>会议名称</label>
			    <div class="col-sm-7">
			      <input type="text" name="theme" class="form-control" id="inputTell" placeholder="全国省市级感染疾病研讨会">
			    </div>
			</div>
			<div class="form-group">
			    <label for="" class="col-sm-2 control-label">会议主题</label>
			    <div class="col-sm-7">
			      <input type="text" name="theme" class="form-control" id="inputTell" placeholder="全国省市级感染疾病研讨会">
			    </div>
			</div>
			<div class="form-group">
			    <label for="" class="col-sm-2 control-label"><span style="color: red">* </span>会议时间</label>
			    <div class="col-sm-3">
			      <input type="text" name="stime" class="form-control" id="inputEmail3" placeholder="2016-5-10">
			    </div>
			    <div class="col-sm-1">&nbsp&nbsp至</div>
			    <div class="col-sm-3">
			    	<input type="text" name="etime" class="form-control" id="inputEmail3" placeholder="2016-5-10">
			    </div>
			</div>
			<div class="form-group">
			    <label for="" class="col-sm-2 control-label"><span style="color: red">* </span>组织机构</label>
			    <div class="col-sm-3">
			      	<select class="form-control">
						<option>主办单位</option>
						<option>主办人员</option>
						<option>主办时间</option>
						<option>主办地点</option>
					</select>
			    </div>
			     <div class="col-sm-1">
			      	
			    </div>
			     <div class="col-sm-3">
			      	<select class="form-control">
						<option>清华学院</option>
						<option>北京医学院</option>
						<option>北京大学</option>
						<option>燕京大学</option>
					</select>
			    </div>
			</div>
			<div class="form-group">
			    <label for="" class="col-sm-2 control-label"><span style="color: red">* </span>会议地点</label>
			     <div class="col-sm-3">
			      	<select name="province" class="form-control">
						<option>北京</option>
						<option>南京</option>
						<option>西安</option>
						<option>上海</option>
					</select>
			    </div>
			     <div class="col-sm-1">
			      	
			    </div>
			     <div class="col-sm-3">
			      	<select name="city" class="form-control">
						<option>东城区</option>
						<option>西城区</option>
						<option>朝阳区</option>
						<option>大兴</option>
					</select>
			    </div>
			</div>
			<div class="form-group">
			    <label for="" class="col-sm-2 control-label">会议人数</label>
			    <div class="col-sm-7">
			      <input type="email" name="takepart_num" value="0" class="form-control" id="inputTell" placeholder="80">
			    </div>
			</div>
		    <div class="btncon">
		      <button type="button" class="btn btn-default btn-warning save">保&nbsp存</button>
		      <button type="button" class="btn btn-default" id="cancel">取&nbsp消</button>
		      <!-- <button class="btn btn-default btn-danger">删&nbsp除</button> -->
		    </div>
		</form>

	</div>     
</div>
<script type="text/javascript" src="../../../js/common/footer.js"></script>
</body>
<script src="../../../js/jquery.min.js"></script>
<!-- 引入项目中带有左侧菜单的公共js -->
<script src="../../../js/common/main.js"></script>
<script type="text/javascript">
$(function(){
	// 获取左侧菜单列表数据(参数为请求地址)
	initMenu('../../../js/data/administrator.json', '活动管理'); 

	//设置保存按钮点击事件
	$('.save').on('click',function(){
		myAlert("已保存");
	})

	//设置取消按钮点击事件
	$('#cancel').click(function() {
	    	$("input[type=reset]").trigger("click")
	});
});




</script>
</html>
