html,body{
    /*color: #ffffff;*/
    background-color: #fff;
}
.contain{
    padding:0 14px; 
    margin-bottom: 10px;   
}

/*导航菜单样式*/
.navMenu{
    position: fixed;
    top: 58px;
    right: 5px;
}
.menuImg{
    width: 30px;
    height: 30px;
    /*background-color: #ffa319;*/
    margin-left: 62px;
}
.menuImg img{
    width: 100%;
    height: 100%; 
}
.menuList{
    border: 1px solid #797979;
    background: linear-gradient(to right, #fff8ef , #ffa319);
    border-radius: 5px;
}
.menuList li{
    height: 25px;
    text-align: center;
    width: 70px;
    line-height: 25px;
    /*background-color: #ffa319;*/
    padding: 5px 10px;
    border-bottom: 1px solid #797979;
}
.menuList .menuApply{
    border-bottom: none;
}
/*导航头样式及图片*/
.share{
    width: 40px;
    position: absolute;
    top: -20px;
    right: 2px;
}
.share img{
    width: 26px;
}
.code{
    margin-top: 20px;
}
.inviImg {
    width: 100%;
    background: url("../../img/app/invitation.jpg") repeat-y 0 0;
    background-size: 100%;
    margin-top: 49px;
    /*height: 100%;*/
    height: 174px;
    text-align: center;
}
.themeName{
    width: 80%;
    margin: 0 auto;
    padding-top: 15px;
    font-size: 18px;
    font-weight: 700;
    height: 72px;
    overflow: hidden;
    line-height: 23px;
    color: #fff;
}
.themeTiem{
    color: #fff;
}
.themeLocation{
    width: 70%;
    margin: 10px auto;
    margin-top: 30px;
    color: #fff;
}

.inviImg img{
    height: 100%;
    width: 100%;
}

/*邀请函首页样式*/
h4{
    color: #f9752b;
}
.designation{
    border-bottom: 1px solid #f9752b;
    padding-bottom: 10px;
    font-size: 16px;
    font-weight: 700;
}
.meetFrist{
    /*border-bottom: 1px solid #f9752b;*/
    padding: 10px 0;      
}
.theme{
    margin-top: 10px;
    display: flex;
}
.conFlex{
    display: flex;
}
.flexOne{
    flex: 1;
}
.flexFive{
    flex: 5;
}
.time{
    margin: 10px 0;
}

.middleCir{
    height: 8px;
    width: 8px;
    display: inline-block;
    border-radius: 50%;
    vertical-align: middle;
    background-color: #f9752b;
    margin: -2px 5%;
}
.leftLine, .rightLine{
    vertical-align: middle;
    display: inline-block;
    width: 42%;
    height: 1px;
    border-bottom: 1px dashed #f9752b;
}
.bottomLine {
    margin-top: 10px;
}

/*会议简介样式*/
.intro *{
    font-size: 14px !important;
}
.introTil{
    width: 80px;
    margin: 0 auto;
    text-align: center;
    margin-bottom: 10px;
}
.pachytene{
    display: inline-block;
    width: 48%;
    height: 4px;
    background-color: #f9752b;
}
.vacancy{
   width: 3%;
   display: inline-block;
}
.rightPachytene{
    background-color: #999;
}
/*组织机构样式*/
.triangle-right {
    /*绘制三角形*/
    width: 0;
    height: 0;
    border-top: 10px solid transparent;
    border-left: 12px solid #f9752b;
    border-bottom: 10px solid transparent;
    display: inline-block;
    vertical-align: middle;
    margin-top: 10px;
}
.triangleOne{
    opacity: 0.7;
}
.institutionName{
    display: inline-block;
    vertical-align: middle;
    margin-left: 8px;
    font-weight: 700;
    margin-top: 10px;
}
.instCon{
    margin-left: 32px;
    margin-top: 8px;
}
.distance{
    margin-top: 20px;
}
/*嘉宾介绍样式*/
.guestDetail{
    display: flex;
}
.guestImg{
    flex: 1;
    margin-right: 5px;
}
.guestImg img{
    width: 70px;
    height: 70px;
    border-radius: 5px;
}
.guestMatter{
    flex: 3;
}
.guestName{
    font-weight: 700;
}
.roll{
    height: 40px;
    line-height: 20px;
    overflow: auto;
    margin-top: 7px;
}
/*活动日程样式*/
.scheduleTime{
    display: flex;
    margin-left: 32px;
    margin-top: 10px;
}
.schTime{
    color: #f9752b;
}
.schDetDistance{
    margin-top: 6px;
}
.schDetTime{
    flex: 1.2;
    color: #f9752b;
}
.schDetCon{
    flex: 2;
}
/*报名页样式*/
.meetApply{
    border-top: 1px solid #f9752b;
    text-align: center;
}
.apply{
    display: inline-block;
    text-align: center;
    line-height: 40px;
    width: 200px;
    height: 40px;
    background-color: #f9752b;
    border: 1px solid #f9752b;
    margin-top: 20px;
    border-radius: 5px;
    color: #fff;
    font-weight: 700;
    font-size: 16px;
}
.QRcode img{
    width: 160px;
    height: 160px;
    margin: 20px auto;
}
.attention{
    color: #000;
}
.hospitality{
    margin-top: 30px;
    font-size: 24px;
    color: #f9752b;
    font-weight: 600;
}