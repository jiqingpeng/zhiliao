function initPage(url,data,fn){
    $.myAjax({
        url: url,
        data: data,
        success: function(result){
            // console.log(result);
            if(result.flag){
                if(fn){
                   fn(result); 
                }
            }else{
                myAlert(result.msg);
            }
        },
        error: function(){
            myConfirm("请求失败!");
        }
    }) 
}

