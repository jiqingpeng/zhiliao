			$('.oLoginList').css('display','block');
		});
		//登陆
		$('#login').click(function(){
			login();
		});
		function login(){
			url=basePath+"user/login/check.do";
			var mobile = $('.user').val();
			var pwd=$('.password').val();
			var pwdMd5=$.md5($('.password').val());
			// 验证手机号
			if (mobile == null || mobile == '') {
				myAlert('账号不能为空!','slow');
				return false;
			}else{
				if(!(/^1[34578]\d{9}$/.test(mobile))){ 
		        	myAlert('账号格式不正确！','slow');  
		        	return false; 
		        };
			};
			// 验证密码
			if (pwd == null || pwd == '') {
				myAlert('密码不能为空!','slow');
				return false;
			};
			$.myAjax({
				url: url,
				data: {
					mobile: mobile,
					pwd:pwdMd5 
					// phone: '13633334444',
					// pwd:'asd' 
				},
				success:function(result){
					console.log(result)
					if(result.flag){
						window.location.href='../user/activityList.html';
					}else{
						// $('.login_message').css('display','block')
						// $('.login_message').html(result.msg)
						myAlert(result.msg,'slow');
					};
				}
			});
		};
		//验证码显示
		// $('.password').focus(function(){
		// 	var user=$('.user').val();
		// 	if(user!=''){
		// 		$('.regCode').removeClass('active');
		// 	}
		// });
		//键盘登陆
		$(document).keypress(function(e) {  
			// 回车键事件  
	        if(e.which == 13) {
	        	login(); 
	        };	  
		});
		//注销
		loginOut('#login_out');
	});
</script>
