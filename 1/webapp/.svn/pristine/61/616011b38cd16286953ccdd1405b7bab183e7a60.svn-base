@charset "utf-8";

/* 页面公共部分样式 */
li a {
	color: #122e67;
}
li a:hover {
	color: #ff8400;
}
li a:visited {
	color: #122e67;
}
.mops-con {
	
}
.inlblk {
	display: inline-block;
}
.con-nav {
    font-size:18px;
	color:#333;
	height: 45px;
	line-height: 45px;
	border-bottom:1px #c9924b solid; 
	margin-bottom: 20px;
	padding-left: 14px;
}
.con-box {
	margin-left: 3%;
}

/* 表格添加或编辑统一样式 */
.tableAdd {
    min-height: 250px;
    border: 1px solid #ddd;
    border-radius: 4px;
    margin-bottom: 14px;
    width: 950px;
    padding-bottom: 14px;
}
.tabAddTitle {
    height: 40px;
    line-height: 40px;
    border-bottom: 1px solid #ddd;
    padding-left: 14px;
    font-weight: bold;
    background-color: #f1f1f1;
    margin-bottom: 14px;
}
/* 页面公共部分样式 --END */


/* 活动列表部分 */
.activityList {
	width: 100%;
	height: 98%;
	min-height: 560px;
}
.activityList li {
	white-space:nowrap;
	list-style: none;
	height: 38px;
	line-height: 38px;
}
.actstime, .place {
	font-size: 14px;
	margin-right: 12px;
	display: inline-block;
	height: 22px;
	padding-right: 12px;
	border-right: 2px solid #ccc;
	line-height: 22px;
	color: #122e67;
}
.actType{
	display: inline-block;
	vertical-align: top;
	width:380px;
	overflow: hidden;
	text-overflow:ellipsis ;
}
.cancel-btn{
	padding:0 5px;
	width:66px;
	text-align: center;
	margin-left:600px;
	border:1px solid #ff6600;
	background:#ff6600;
	color:#fff;
	height:24px;
	border-radius: 4px;
}
.cancel-btn:hover{
	cursor:pointer;
}
.actstime {
	width: 78px;
}
.place {
	width: 68px;
	text-align: center;
}
/* 活动列表部分-end */

/* 管理员配置部分 */
.admindel {
	margin-right: 10px;
}
/* 管理员配置部分-END */








