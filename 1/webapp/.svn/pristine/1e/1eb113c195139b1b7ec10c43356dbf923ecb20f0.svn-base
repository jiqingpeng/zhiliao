.wrap {
    width: 1110px;
    margin: 0 auto;
    font-size: 12px
}

.wrap .active {
    display: none
}

.wrap .progress {
    margin-bottom: 20px
}

.wrap .progress span {
    float: left;
    vertical-align: middle
}

.wrap .progress span.txt {
    padding-right: 15px;
    background: url(../../img/index/pbook.png) no-repeat 58px -9px;
    color: #2ca1e7
}

.wrap .progress span.grayicon {
    background: url(../../img/index/pbook.png) no-repeat 58px 7px
}

.wrap .progress span.graytxt {
    color: #ccc;
    background: url(../../img/index/pbook.png) no-repeat 58px 7px
}

.wrap .progress span.line {
    margin-top: 6px;
    border-bottom: 2px solid #83d0fe;
    padding-top: 3px
}

.wrap .progress span.gray {
    border-bottom-color: #ccc
}

.wrap .progress span.nobg {
    background: 0 0;
    padding-right: 0
}

.wrap .progress em {
    float: left;
    width: 10px;
    height: 10px;
    margin: 6px 6px 0 -8px;
    display: block;
    background: url(../../img/index/pbook.png) no-repeat 0 -47px
}

.wrap .progress em.gray {
    background-position: 0 -32px
}

.wrap .content .main {
    width: 1110px
}

.wrap .content .main .info .infoBox {
    width: 660px;
    padding: 0 60px 20px;
    border: 1px solid #ccc;
    margin-bottom: 20px;
    border-top: 3px solid #2da1e7
}
.wrap .content .main .info .infoBox .trains{
   
    font-size:22px;
    margin-top:60px;
    margin-right:107px;
}
.wrap .content .main .info .infoBox .trainInfo{
   
    margin-top:20px;
    font-size:16px;
    color:#666;
}
.wrap .content .main .info .infoBox .trainInfo p{
    margin:10px 0;
}
.wrap .content .main .info .infoBox .trainInfo .date,.time{
    font-weight: bold;
}
.wrap .content .main .info .infoBox .trainInfo .txtright{
    text-align: right;
}
.wrap .content .main .info .infoBox .img{
    
    margin:34px 50px 0;

}
.wrap .content .main .info .infoBox .img .icon{
    
}
.wrap .content .main .info .infoBox .img .txt{
    font-size:18px;
    text-align:center;
}
.wrap .content .main .info .infoBox .img .icon .iconImg{
    display: inline-block;
    width:40px;
    height:50px;
    background:url(../../img/index/train-icon.png) no-repeat -245px -80px;
}
.wrap .content .main .info .infoBox .img .icon .line{
    display: inline-block;
    width:30px;
    height:25px;
    border-top:2px solid #ddd;
    
}
.wrap .content .main .info .infoBox .infoTit {
    padding: 24px 0 22px;
    color: #666;
    font-size: 16px;
    font-weight: 700
}
.wrap .content .main .info .infoBox .infoTit .txt{
    display: inline-block;
    height:16px;
    line-height: 16px;
    font-size:12px;
}
.wrap .content .main .info .infoBox .ticketSize{
    width:145px;
    height:50px;
    padding-top:10px;
    margin:10px 10px 0;
    border:1px solid #ddd;
    color:#666;
    font-size:14px;
}
.wrap .content .main .info .infoBox .ticketSize:hover{
    cursor: pointer;
}
.wrap .content .main .info .infoBox .ticketSizeOn{
    border:none;
    background:url(../../img/index/train-icon.png) no-repeat -10px -9px;
}
.wrap .content .main .info .infoBox .ticketSize p{
    text-align: center;
}
.wrap .content .main .info .infoBox .ticketSize p .f63{
    color:#f63;
}
.wrap .content .main .info .infoBox .ticketSize .last{
    text-align:center;
    margin-top:5px;
}
.wrap .content .main .info .infoBox .signin {
    padding-bottom: 16px;
    border-bottom: 1px solid #ddd;
    color: #333
}
.wrap .content .main .info .infoBox .noMess{
   margin-top:10px;
   margin-left:10px;
}
.wrap .content .main .info .infoBox .noMess input{
    vertical-align: middle;
}

.wrap .content .main .info .infoBox .row {
    padding-bottom: 10px;
    height: 34px;
    line-height: 34px;
    position: relative
}

.wrap .content .main .info .infoBox .row input {
    margin: 1px 0;
    padding: 7px 10px
}

.wrap .content .main .info .infoBox .row .name {
    margin-left: 80px;
    position: relative
}

.wrap .content .main .info .infoBox .row .name input {
    width: 210px
}

.wrap .content .main .info .infoBox .row .selectHead {
    display: inline-block;
    height: 32px;
    background: #fff;
    line-height: 32px;
    border: 1px solid #ddd;
    border-radius: 2px 2px 0 0;
    margin-left: 80px;
    position:relative;
}
.wrap .content .main .info .infoBox .row .selectHead:hover{
    cursor: pointer;
}
.wrap .content .main .info .infoBox .row .selectHead .selectBody{
    position: absolute;
    left:0;
    top:34px;
    width:100%;
    max-height: 350px;
    border:1px solid #ddd;
    z-index:1;
    background: #fff;
}
.wrap .content .main .info .infoBox .row .selectHead .selectBody li{
    padding-left:10px;
}
.wrap .content .main .info .infoBox .row .selectHead .selectBody li:hover{
    background: #2ca1e7;
    color:#fff;
}
.wrap .content .main .info .infoBox .row .selectHead .selectArrow {
    width: 20px;
    height: 20px;
    float: right;
    margin: 6px 6px 0 0;
    background: url(../../img/index/pbook.png) 4px -140px no-repeat #e0e0e0
}

.wrap .content .main .info .infoBox .row .selectHead .selectTxt {
    padding-left: 10px;
    margin-right: 30px;
    text-overflow: ellipsis;
    white-space: nowrap
}

.wrap .content .main .info .infoBox .row .codeId {
    margin-left: 20px;
    position: relative
}

.wrap .content .main .info .infoBox .row .codeId input {
    width: 278px
}

.wrap .content .main .info .infoBox .row .passPhone {
    width: 402px;
    margin-left: 80px
}

.wrap .content .main .info .infoBox .row .prompt {
    margin-left: 66px;
    position: relative
}

.wrap .content .main .info .infoBox .row .prompt .delIocn {
    display: inline-block;
    position: absolute;
    right: -36px;
    top: 7px;
    width: 20px;
    height: 20px;
    background: url(../../img/index/pbook.png);
    background-position:-21px -27px;
}

.wrap .content .main .info .infoBox .row .prompt .promptNum {
    position: absolute;
    left: -352px;
    top: 0;
    display: inline-block;
    width: 20px;
    height: 20px;
    font-size: 16px
}

.wrap .content .main .info .infoBox .row .tipIcon {
    width: 20px;
    height: 20px;
    position: absolute;
    right: 8px;
    top: 6px;
    background: url(../../img/index/pbook.png) no-repeat -21px -83px
}

.wrap .content .main .info .infoBox .row .quesIcon {
    width: 20px;
    height: 20px;
    position: absolute;
    right: 8px;
    top: 6px;
    background: url(../../img/index/pbook.png) no-repeat -21px -110px
}

.wrap .content .main .info .infoBox .passengerContent .passInfo {
    margin-bottom: 30px;
    margin-top: 20px;
    border-left: 4px solid #fff
}

.wrap .content .main .info .infoBox .passengerContent .passInfo:hover {
    border-left: 4px solid #83d0fe;
};

.wrap .content .main .info .infoBox .passengerContent .newBtn {
    margin-left: 80px
}

.wrap .content .main .info .infoBox .passengerContent .newBtn a {
    display: inline-block;
    height: 34px;
    line-height: 34px;
    background: #ffb44a;
    border-radius: 3px;
    text-align: center;
    font-size: 16px;
    color: #fff;
    width: 144px;
    margin-left:80px;
}

.wrap .content .main .info .infoBox .passengerContent .newBtn a:before {
    width: 22px;
    height: 22px;
    display: inline-block;
    margin-top: 6px;
    margin-right: 6px;
    vertical-align: top;
    background: url(../../img/index/pbook.png) no-repeat -49px -267px;
    content: "";
}

.wrap .content .main .info .infoBox .passengerContent .insurance {
    border-top: 1px solid #ccc;
    padding-top: 20px;
    margin-top: 20px
}

.wrap .content .main .info .infoBox .passengerContent .insurance .txt {
    display: inline-block;
    width: 64px;
    margin-left: 80px
}

.wrap .content .main .info .infoBox .passengerContent .insurance .txtr {
    margin-left: 10px
}

.wrap .content .main .info .infoBox .contact .txt {
    display: inline-block;
    width: 64px
}

.wrap .content .main .info .infoBox .contact .selectHead {
    margin-left: 6px
}

.wrap .content .main .info .infoBox .contact .row input {
    width: 336px
}

.wrap .content .main .info .check {
    border-top: 3px solid #2da1e7;
    margin: 20px 0
}

.wrap .content .main .info .check p {
    text-align: center;
    margin-top: 20px
}

.wrap .content .main .info .check p .radio {
    display: inline-block;
    width: 18px;
    height: 18px;
    margin-top: -4px;
    margin-right: 6px;
    vertical-align: middle;
    background: url(../../img/index/pbook.png) no-repeat -89px -202px
}

.wrap .content .main .info .check p a {
    color: #2da1e7
}

.wrap .content .main .info .saveBtn {
    width: 160px;
    height: 42px;
    line-height: 42px;
    margin: 0 auto;
    text-align: center;
    background: #ff7800;
    color: #fff;
    border: 1px solid #ff7800;
    border-radius: 4px;
    font-size: 20px
}

